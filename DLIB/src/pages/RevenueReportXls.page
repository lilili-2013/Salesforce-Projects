<apex:page showHeader="false" sidebar="false" controller="RevenueReportPrint" contenttype="application/vnd.ms-excel#RevenueReport.{!reportDate}.xls">
  	<table>
  		<tr>
  			<th>Project Name</th>
  			<th>Billable Amount</th>
  			<th>Outstanding Adjustments</th>
  			<th>Deferred Revenue</th>
  		</tr>
  		<apex:repeat value="{!reportData}" var="item">
  			<tr>
		  		<td>{!item.project.Name}</td>
		  		<td><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!item.BillableAmount}"/></apex:outputText></td>
		  		<td><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!item.TotalInvoice}"/></apex:outputText></td>
		  		<td><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!item.Revenue}"/></apex:outputText></td>
		  	</tr>
  		</apex:repeat>
	  	<tr>
	  		<th>Total</th>
	  		<th><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!totalBA}"/></apex:outputText></th>
	  		<th><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!totalAdj}"/></apex:outputText></th>
	  		<th><apex:outputText value="{0, number, #,###,##0.00}"><apex:param value="{!totalRev}"/></apex:outputText></th>
	  	</tr>
  	</table>
</apex:page>