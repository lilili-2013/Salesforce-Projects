<apex:page standardController="pse__Proj__c" extensions="ngForceController3" showHeader="false" standardStylesheets="false">
 
  <head>
    <meta charset="utf-8"></meta>
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width"></meta>
    <title>psaInvoices</title>
    <link href="{!URLFOR($Resource.psaInvoices, 'styles/lib.css')}" rel="stylesheet"></link>
    <link href="{!URLFOR($Resource.psaInvoices, 'main.css')}" rel="stylesheet"></link>
    <style>
    	h3.title {
    		margin-top:0px;
    	}
    </style>
  </head>
  <body>
    <script>
      var sf = {};
      sf.projId = '{!$CurrentPage.parameters.projId}';
      if(sf.projId == null || sf.projId.length == 0) sf.projId = '{!proj.Id}';
		
		function redirectPage(view) {
			console.log(view);
			location.href = '/apex/psaMilestones?projId='+sf.projId+'#/'+sf.projId+'/'+view;
		}
    </script>
    <div ng-app="psamilestones" ng-controller="AppCtrl">
      <loading-view is-loading="{{isLoading}}"></loading-view>
      <div class="container-fluid" ng-view="ng-view"></div>
    </div>
    <script src="{!URLFOR($Resource.psaInvoices, 'lib.js')}"></script>
    <script src="{!URLFOR($Resource.psaInvoices, 'templatesI.js')}"></script>
    <script src="{!URLFOR($Resource.psaInvoices, 'mainI.js')}"></script>
  </body>
	
</apex:page>