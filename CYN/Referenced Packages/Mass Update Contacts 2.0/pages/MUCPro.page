<apex:page standardcontroller="Account" extensions="mucpro.MUCProExtension"
	tabstyle="Contact">



	<apex:sectionHeader title="{!$Label.Update_Contact_Addresses}"
		help="http://www.x2od.com/mucp" />

	<apex:pagemessages />
	<apex:actionStatus id="selectstatus">
		<apex:facet name="start">
			<c:updaterstatus BackColor="#FFFFFF" borderColor="#000000"
				borderSize="3" height="75px" width="75px"
				ImageUrl="{!URLFOR($Resource.mucpro__AjaxStatus)}" message="Loading..." />
		</apex:facet>
	</apex:actionStatus>

	<apex:form rendered="{!IsValid}">

		<c:MUCPDisplay />

	</apex:form>

	<c:MUCPError rendered="{!isvalid == false}" />
</apex:page>