<apex:page standardController="Contact" recordSetVar="contacts" >
     <apex:form >
       <apex:dataTable value="{!contacts}" var="c">
           <apex:column value="{!c.FirstName}"/>
                      <apex:column value="{!c.LastName}"/>
                                 <apex:column value="{!c.Email}"/>
       </apex:dataTable>
     </apex:form>
</apex:page>