									<ul>  
										<li class="Lgray">
											<h3><a target="_Blank" href="{!minvoices.DocumentId}" ><strong>{!minvoices.DisplayText}</strong></a></h3>
										</li>
									</ul>
								</div>
							</apex:outputPanel>
						</apex:repeat>
					</apex:outputpanel>
					
					<apex:outputpanel rendered="{!If(mapPolicyIdToRecentInvoicesCount[indPoliciesMob.policyId] == 0, true,false)}">
						<div class="mobile-policy-container">
							<ul>
								<li class="Lgray">
									<p>You do not have any recent invoices</p>
								</li>
							</ul>
						</div>
					</apex:outputpanel>
					
					<div class="mobile-policy-container">
					   <ul>  
							<li class="Tgray">
								<h2>Future Payment Dates</h2>
							</li>
						</ul>
					</div>
					
					<apex:outputpanel rendered="{!if(lstFuturePayments.size > 0,true,false)}">
						<apex:repeat value="{!lstFuturePayments}" var="mFutPayments">
							<apex:outputPanel rendered="{!if(mFutPayments.PolicyId == indPoliciesMob.policyId,true,false)}">
								<div class="mobile-policy-container">
									<ul>  
										<li class="Lgray">
											<h3>ESTIMATED DUE DATE</h3>
											<p><apex:outputText value="{0,date,MM/dd/yy}"><apex:param value="{!mFutPayments.EstimatedDate}" />
											</apex:outputText></p>
										</li>
										<li class="Dgray">
											<h3>MINIMUM DUE</h3>
											<p><apex:outputText value="${0, number,###,##0.00}"   >
												<apex:param value="{!mFutPayments.MinDue}"/>
											</apex:outputText></p>
										</li>
								</ul>
								</div>
							</apex:outputpanel>
						</apex:repeat>
					</apex:outputpanel>
					
					<apex:outputpanel rendered="{!If(mapPolicyIdToFuturePaymentDateCount[indPoliciesMob.policyId] != 0, false, true)}" >
						<div class="mobile-policy-container">
							<ul> 
								<li class="Lgray">
									<h3>You do not have any payments due at this time</h3>
								</li>
							</ul>
						</div>
					</apex:outputpanel>
					
					
				</apex:outputPanel>
				</apex:repeat>
				</div>
			
			</div>
			
			<!-- End Primary Billing Account  -->
			
			<!-- Begin Payments Credits History -->
			<div class="default-full-table row-fluid">
				<div class="span12">   
					<div class="table-header arrow-container">
						<h1 class="table-h1-title">Payments &amp; Credits History</h1>
						<div class="rotate-caret mobile"><i class="fa fa-caret-right fa-2x"></i></div>
					</div>
			
						<!-- Begin mobile table ("desktop" version is removed on mobile, this is displayed.) if none, hide.-->
					
						<div class="mobile-expandable-div policies-mobile">
							
							<apex:outputpanel rendered="{!If(lstPaymentCredits.size > 0, true, false)}">
								<apex:repeat value="{!lstPaymentCredits}" var="MpaymentCredits">
									<div class="mobile-policy-container">
										<ul>   
											<li class="Lgray">
												<h3>PAYMENT TYPE</h3>
												<p>{!MpaymentCredits.TransactionType}</p>
											</li>
											<li class="Dgray">
												<h3>TRANSACTION</h3>
												<p><apex:outputText value="{0,date,MM'/'dd'/'yy}">
														<apex:param value="{!MpaymentCredits.TransactionDate}" /> 
													</apex:outputText>
												</p>
											</li>
											<li class="Lgray">
												<h3>AMOUNT</h3>
												<p><apex:outputText value="${0, number,###,##0.00}"   >
													<apex:param value="{!MpaymentCredits.Amount}"/>
												</apex:outputText></p>
											</li>
										</ul>
									</div>
								</apex:repeat>
							</apex:outputpanel>
							
							<apex:outputpanel rendered="{!If(lstPaymentCredits.size > 0, false, true)}">
								<div class="mobile-policy-container">
									<ul>
										<li class="Lgray" style="display:{!If(lstPaymentCredits.size != 0, 'none', '')}; ">
											<h3>You do not have any payments credits at this time</h3>
										</li>
									</ul>
								</div>
							</apex:outputpanel>
						</div>
			
						<table class="table policies-desktop table-striped">
							<thead>
								<apex:outputpanel rendered="{!if(lstPaymentCredits.size > 0,true,false)}">
									<tr>
										<th class="description">Payment Type</th>
										<th class="number">Transaction Date</th>
										<th>Amount</th>
									</tr>
								</apex:outputpanel>
							</thead>
							<tbody>
								<apex:repeat value="{!lstPaymentCredits}" var="paymentCredits">
									<tr>
										<td><strong><apex:outputText value="{!if(paymentCredits.TransactionType == 'Check', paymentCredits.TransactionType+' '+paymentCredits.CheckNumber,paymentCredits.TransactionType)}"/> </strong></td>
										<td><apex:outputText value="{0,date,MM'/'dd'/'yy}">
												<apex:param value="{!paymentCredits.TransactionDate}" /> 
											</apex:outputText>
										</td>
										<td><apex:outputText value="${0, number,###,##0.00}"   >
											<apex:param value="{!paymentCredits.Amount}"/>
										</apex:outputText></td>
									</tr>             
								</apex:repeat>
								<tr style="width:100%">
									<td class="center-icon" style="width:100%; display:{!If(lstPaymentCredits.size != 0, 'none', '')}; ">
										<span > Your history is currently unavailable at this time</span>
									</td>
								</tr>
							</tbody>
						</table>
					</div>
				</div>
			
			<!-- End Payments Credits History -->
					<c:SL_SiteFooterComponent />
			</div>  <!-- main-content div End -->
			
			
		</apex:define>
		<!---------- End of Main section ----------------->
		
		<!----- Start of Right side  bar section --------->
		<apex:define name="rightSideBar" >
			<div id="sidebar" class="span3" style="display:{!If(isError == true, 'none', '')};">
				<c:SL_SideBarComponents />
			</div>
		</apex:define>
		<!----- End of Right side  bar section --------->
		
	</apex:composition>

</apex:page>