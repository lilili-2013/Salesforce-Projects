<apex:page showHeader="false" sidebar="false" standardStylesheets="false" controller="SL_PolicyDetailPage" action="{!pageAction}">
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	 <meta name="viewport" content="width=device-width, initial-scale=1"/>
	<!-- Start of Page -->
	<apex:composition template="SL_SiteTemplate">
		
        <!-- Start of Main section -->  
        <apex:define name="mainSection">
        	<style>
	       		.message
				{
					display: none;
				}
				
				.messageCell
				{
					width: 80%;
				}

                a[disabled] {
                    pointer-events:none;
                    color:grey;
                }
        	</style>
			<apex:form >
					<c:SL_PureJSScripts />
					<apex:pageMessages />
					<div style="visibility: visible;color: red;" id="errorTextId" align="center"/>
					
					<script>
						if($('.messageText').text() != '')
						{
							var strError = $('.messageText').text();
							SL_PureJSUtil.registerShowErrorMessage(strError.split(/:([^:]+)$/)[1]);
						}
					</script>
			</apex:form>
			<script>
					
				function loadAnotherpolicy(value)
				{
					if(value != 'Select')
					{
						if('{!$Profile.Name}' !== 'Pure Customer Community Login User')
							window.open('/BrokerView/PolicyDetailPage?url='+value+'&mId={!$CurrentPage.parameters.mId}', '_self');
						else
							window.open('/PolicyDetailPage?url='+value, '_self'); 
					} 
				}
				
				$(document).ready(function(){
			
					if('{!isError}' == 'true')
					{
						//document.getElementById('main-nav').style.display = 'none';
						$('.main-navClass').hide();
						$('#mobile-button').hide();
					}
					
				});
				
			</script>
<!-- 			<apex:outputpanel rendered="{!!isError}" > -->
			<!-- 	Start of common section -->
        	<div id="main-content" class="span9" style="margin-left: -1em;margin-top: -1.5em;display:{!If(isError == true, 'none', '')};"> 
        		
        		<!-- Auto Policy header --> 
        		<apex:outputpanel rendered="{!If(policyType == 'auto',true,false)}">
                	<h1  class="welcome"><div style="width:95%; word-wrap: break-word;">{!sObjPolicy.Description_Display__c}</div></h1>
        		</apex:outputpanel>
        		
        		<!-- Watercraft Policy header --> 
        		<apex:outputpanel rendered="{!If(policyType == 'watercraft',true,false)}">
                	<h1  class="welcome"><div style="width:95%; word-wrap: break-word;">{!sObjPolicy.Description_Display__c}</div></h1>
        		</apex:outputpanel>
        		
        		<!-- Excess Policy header --> 
        		<apex:outputpanel rendered="{!If(policyType == 'excess',true,false)}">
                	<h1  class="welcome"><div style="width:95%; word-wrap: break-word;">{!sObjPolicy.Description_Display__c}</div></h1>
        		</apex:outputpanel>  
        		
        		<!-- Collections Policy header --> 
        		<apex:outputpanel rendered="{!If(policyType == 'collections',true,false)}">
                	<h1  class="welcome"><div style="width:95%; word-wrap: break-word;">{!sObjPolicy.Description_Display__c}</div></h1>
        		</apex:outputpanel>
                
                <!-- Collections Policy header --> 
        		<apex:outputpanel rendered="{!If(policyType == 'homeowners',true,false)}">
