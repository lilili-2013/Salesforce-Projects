							<apex:selectList onchange="loadAnotherpolicy(this.value);"  value="{!strSelectedPolicy}" multiselect="false" size="1">
		            				<apex:selectOptions value="{!lstpolicies}" />
		        			</apex:selectList>
	        			</apex:outputpanel>
	        		</apex:form>
	                
            	</div>
