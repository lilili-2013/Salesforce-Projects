<!-- 
*   	M2M2_Page is VF page used to render the many to many UI.
*		Custom Controller usage.  Required URL parameters:
*			1. cn=<config name>
*			2. id=<primary parent id>
*			3. fn=<primary parent name>
*		Example URL:
*			/apex/M2M2_Page?cn=CompanyToBank&id=001E000000NKGpu&fn=MyTestAccount
*	
*		Author  :   Wilson Ng
*		Date	:	October 9, 2012
*  
-->
<apex:page controller="M2M2_Controller"> 

	<c:M2M2_Component M2M2_Controller="{!controller}" />

</apex:page>