		function activateForm() {
		    //activeForm = $('#submit-claim-type input:checked').val();
		 
		    if( activeForm == 'auto') {
				resetForms("#formAuto");
		    } else if ( activeForm == 'autoglass') {
				//to do
		    } else if ( activeForm == 'homeowners') {
				resetForms("#formHomeowners");
			} else if ( activeForm == 'collections') {
				resetForms("#formCollections");
			} else if ( activeForm == 'watercraft') {
				resetForms("#formWatercraft");
			} else if ( activeForm == 'flood') {
				resetForms("#formFlood");
			} else if ( activeForm == 'workers_compensation') {
				resetForms("#formWorkerCompensation");
			}
		};
		
		function resetForms(eve) {
		    
		    //var arr = $('#form-auto').val($("input[placeholder]").attr("placeholder"));
		    $(eve+" input[Type='text']").each(function(){

		    	if($(this).attr('id').indexOf('name') != -1)
		    	{	
		    		$(this).val('{!objAccount[0].Name}'); 
		    	}
		    	if($(this).attr('id').indexOf('relToInsured') != -1)
		    	{	
		    		$(this).val('Self'); 
		    	}
		    	if($(this).attr('id').indexOf('phone') != -1)
		    	{	
		    		$(this).val('{!objAccount[0].Phone}'); 
		    	}
		    	if($(this).attr('id').indexOf('email') != -1)
		    	{	
		    		$(this).val('{!objAccount[0].Primary_Email__c}'); 
		    	}
		    });
		    
		    
		};
		
		function sameAsData(eve)
		{
			//var activeValue = $(this.checked').val();
		    var activeValue = eve.checked;
		    if(activeValue)
		    {	
		    	activateForm();
			}
		}
		function selectedVehicalDetails(selVal)
		{
			var arr = selVal.split(",");
			var activeFormId = '';
			if( activeForm == 'auto') {
				activeFormId = "#autoVehical";
			} else if (activeForm == 'watercraft')
			{
				activeFormId = "#autoWatercraft";
			}	
			
			$(activeFormId+" input[Type='text']").each(function(){
				if($(this).attr('id').indexOf('idMake') != -1) {
					$(this).val(arr[0]);
				}
				if($(this).attr('id').indexOf('idModel') != -1) {
					$(this).val(arr[1]);
				}
				if($(this).attr('id').indexOf('idYear') != -1) {
					$(this).val(arr[2]);
				}
			});
		}
		function validationOnSubmit(eve)
		{
			var flag = false;
			$(eve+" input[Type='text']").each(function(){
				//alert($(this).val());
				if($(this).val() == "") {
					flag = true;
					
				}
			});
			//alert(flag);
			return flag;
		}
	</script>
