<apex:page controller="OSF_Calendar.RequestedMeetingsController" showHeader="false">
<apex:stylesheet value="{!URLFOR($Resource.OSF_Calendar__Styles, 'style.css')}"/>
    <div class="settings-screen">
        <div class="settings-screen-header clearfix">
            <div class="settings-screen-header-left">&nbsp;</div>
            <span>My Meeting Requests</span>
        </div>
         
        <div class="settings-screen-content">
            <apex:pageBlock >
                <apex:pageBlockTable value="{!requestedMeetings}" var="list">
                    <apex:column headerValue="Subject">
                        <a href="{!sfBaseUrl}/{!list.whoid}#/sched/{!URLENCODE($Site.Prefix + '/scheduling/editMeeting.apexp?id=')}{!list.id}" target = "_parent">{!list.subject}</a>
                    </apex:column> 
                </apex:pageBlockTable>
            </apex:pageBlock>
        </div>
    </div>
</apex:page>