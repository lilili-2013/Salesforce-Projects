        <externalId>false</externalId>
        <label>ContactValidationAbsent</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HistoryEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
