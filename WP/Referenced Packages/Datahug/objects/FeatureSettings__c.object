        <externalId>false</externalId>
        <label>HistoryEnabled</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>MeetingsEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Whether or not the calendar tab shows in the DH app</description>
        <externalId>false</externalId>
        <label>MeetingsEnabled</label>
        <type>Checkbox</type>
    </fields>
    <label>FeatureSettings</label>
