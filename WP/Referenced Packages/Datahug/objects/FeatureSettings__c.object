    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AnalyticsEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Whether or not the analytics tab is enabled in the DH app</description>
        <externalId>false</externalId>
        <label>AnalyticsEnabled</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ChatterEnabled__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>True if the DH chatter column is to be displayed in the DH app</description>
        <externalId>false</externalId>
        <label>ChatterEnabled</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>ContactValidationAbsent__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
