				<apex:pageBlockSectionItem >
					<apex:outputText label="Security Token" value="{!securityToken}" id="securityToken"/>
					<apex:commandButton action="{!resetToken}" value="Reset Token" id="resetTokenButton"/>
				</apex:pageBlockSectionItem>
			</apex:pageBlockSection>
		</apex:pageBlock>
