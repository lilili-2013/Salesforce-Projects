	<apex:sectionHeader title="Security Settings" />
	<apex:form >
		<apex:pageMessages id="settingsPageMessages"/>
		<apex:pageBlock title="Tokens" mode="detail">
			<apex:pageBlockSection title="Security Token" showHeader="true" columns="1" collapsible="false">
