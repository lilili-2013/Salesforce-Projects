    public static void createReports(){
        Map<String, List<Object>> reportMap = new Map<String, List<Object>>{
            'Prepay of Extension, Proof.$CU' => new List<Object> {
                new IST__SuperTRUMP_Report__c(IST__Name__c = 'Prepay of Extension, Proof',
                    IST__Description__c = '',
                    IST__Extension__c = '$CU',
                    IST__Report_Group__c = 'Custom'
                    ),
                    ''
            },
            'Progress Payments, Present Value Proof.$CU' => new List<Object> {
                new IST__SuperTRUMP_Report__c(IST__Name__c = 'Progress Payments, Present Value Proof',
                    IST__Description__c = '',
                    IST__Extension__c = '$CU',
                    IST__Report_Group__c = 'Custom'
                    ),
                    ''
            },
            'Progress Payments, Summary.$CU' => new List<Object> {
                new IST__SuperTRUMP_Report__c(IST__Name__c = 'Progress Payments, Summary',
                    IST__Description__c = '',
                    IST__Extension__c = '$CU',
                    IST__Report_Group__c = 'Custom'
                    ),
                    'UHJvZ3Jlc3MgcGF5bWVudHN1bW1hcnkAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAADRARAA'
            }
        };
        List<IST__SuperTRUMP_Report__c> storedReports = [SELECT IST__Name__c, IST__Extension__c FROM IST__SuperTRUMP_Report__c LIMIT 50000];
        Set<String> reportNames = new Set<String>();
        for(IST__SuperTRUMP_Report__c report: storedReports){
            reportNames.add(report.IST__Name__c + '.' + report.IST__Extension__c);
        }
        for(String key: reportMap.keySet()){
            if (reportNames.contains(key)){
                reportMap.remove(key);
                System.debug('Report "' + key + '" is already present.');
            }
            else{
                System.debug('Report "' + key + '" is NOT present.');
            }
        }

        for(List<Object> reportObjects : reportMap.values()){
            IST__SuperTRUMP_Report__c reportDetails = (IST__SuperTRUMP_Report__c) reportObjects[0];
            String encodedReport = (String) reportObjects[1];

            Database.SaveResult result = Database.insert(reportDetails, true);
            Attachment reportAttachment = new Attachment(
                Name = reportDetails.IST__Name__c + '.' + reportDetails.IST__Extension__c,
                ParentId = result.getId(),
                Description = 'Default Report',
                Body = System.EncodingUtil.base64Decode(encodedReport)
                );
            System.debug('Inserting new report: ' + reportAttachment.Name);
            Database.insert(reportAttachment, true);
        }
        
    }
    
