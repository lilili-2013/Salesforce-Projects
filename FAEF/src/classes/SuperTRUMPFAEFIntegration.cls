    public static void createMappings(){
        List<IST__SuperTRUMP_Mapping__c> mappings = new List<IST__SuperTRUMP_Mapping__c>();
        mappings.addAll(getOpsMappings());
        mappings.addAll(getSalesMappings());
        insertMappings(mappings);
    }
    
    private static void insertMappings(List<IST__SuperTRUMP_Mapping__c> mappings){
        List<IST__SuperTRUMP_Mapping__c> storedMappings = [SELECT IST__SF_Object_Name__c, IST__SF_Field_Name__c, IST__ST_Field_Name__c
                                                           FROM IST__SuperTRUMP_Mapping__c LIMIT 10000];
        // Check if mappings already exist.  If so, remove them from the mappings list
        Integer idx = 0;
        while(idx < mappings.size()){
            IST__SuperTRUMP_Mapping__c mapping = mappings[idx];
            boolean isFound = false;
            for(Integer storedIdx = 0; storedIdx < storedMappings.size(); storedIdx++){
                IST__SuperTRUMP_Mapping__c m = storedMappings[storedIdx];
                if (m.IST__SF_Object_Name__c == mapping.IST__SF_Object_Name__c
                    && m.IST__SF_Field_Name__c == mapping.IST__SF_Field_Name__c){
                    mappings.remove(idx);
                    isFound = true;
                    // We remove the stored mappings to speed up future iterations
                    storedMappings.remove(storedIdx);
                    break;
                }
            }
            if (!isFound){
                System.debug('Mapping for ' + mapping.IST__SF_Object_Name__c + '.' + mapping.IST__SF_Field_Name__c + ' was NOT found!');
                idx++;
            }
        }
        if (mappings.size() > 0){
            System.debug(mappings.size() + ' new mappings being created.');
            Database.insert(mappings, true);
        }
    }
    
