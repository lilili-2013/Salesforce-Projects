<apex:page standardController="Monthly_Revenue_Process_Control__c" extensions="SL_MonthlyRevenueProcessController" id="Page">

    <apex:outputPanel id="refresh" rendered="true">                  
        <apex:outputPanel id="refresh1" rendered="{!RefreshPage}">   
            <script>window.top.location='{!strRedirectUrl}';</script>
        </apex:outputPanel>                                          
    </apex:outputPanel>                                                

    <apex:form id="processControl">  
        <apex:PageBlock id="PageBlock" mode="view">                
            <apex:pagemessages ></apex:pagemessages>
            <apex:pageBlockButtons >

                <apex:commandButton action="{!initialReversal}" value="Initial Reversal" disabled="{!!InitialReversalEnabled}"/>
            <!--<apex:commandButton action="{!initialReversal}" value="Initial Reversal" onclick="http://slv-hlwebtest/monthlyrevenueprocessing/default.aspx?action=1;this.form.target='_blank" disabled="{!!InitialReversalEnabled}"/>-->
            <!--<apex:commandButton action="{!initialReversal}" value="Initial Reversal" onclick="http://slv-hlwebtest/monthlyrevenueprocessing/default.aspx?action=1" disabled="{!!InitialReversalEnabled}"/>-->
            <!--<apex:commandButton id="initialReversal" value="Initial Reversal" action="{!initialReversal}" onclick="window.open('http://slv-hlwebtest/monthlyrevenueprocessing/default.aspx?action=1','_blank','height=400,location=no,resizable=yes,toolbar=no,status=no,menubar=no,scrollbars=1', 1);" disabled="{!!InitialReversalEnabled}"/>-->
            <!--<apex:commandButton id="initialReversal" value="Initial Reversal" action="{!initialReversal}" onclick="window.location.href='http://slv-hlwebtest/monthlyrevenueprocessing/default.aspx?action=1'" disabled="{!!InitialReversalEnabled}"/>-->

                <apex:commandButton action="{!unlockAll}" value="Unlock All" rendered="{!RevenueAccrualsLocked}"/>
                <apex:commandButton action="{!lockAll}" value="Lock All" rendered="{!!RevenueAccrualsLocked}" disabled="{!!LockAllEnabled}"/>
                <apex:commandButton action="{!finalReversal}" value="Final Reversal" disabled="{!!FinalReversalEnabled}"/>
                <apex:commandButton action="{!exportRevenue}" value="Export Revenue" disabled="{!!ExportRevenueEnabled}"/>
                <apex:commandButton action="{!earlyLook}" value="Early Look" disabled="{!!EarlyLookEnabled}"/>
                <apex:commandButton action="{!currencyConversion}" value="Currency Conversion" disabled="{!!CurrencyConversionEnabled}"/>
                <apex:commandButton action="{!finalReport}" value="Final Report" disabled="{!!FinalReportEnabled}"/>
                <apex:commandButton action="{!staffSummary}" value="Staff Summary" disabled="{!!StaffSummaryEnabled}"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection columns="1" id="Status">
                <apex:outputText label="Status" value="{!status}"/>
            </apex:pageBlockSection>
        </apex:PageBlock>
    </apex:form>
    
</apex:page>