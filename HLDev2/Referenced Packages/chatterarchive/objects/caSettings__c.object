        <fullName>Display_Followers_in_Email__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Check to show record followers in email</description>
        <externalId>false</externalId>
        <label>Display Followers in Email</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>EnableGroup__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If checked, enables the app to function only for the group name specified.</description>
        <externalId>false</externalId>
        <label>EnableGroup</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Extend_Enable_Group_to_Non_Members__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>When set, the comments sent by users not in the Enabled Group, we check if any person in the group has posted to the comment. If we find one, the comment is treated as if it was posted from someone in the group.</description>
        <externalId>false</externalId>
        <inlineHelpText>When set, the comments sent by users not in the Enabled Group, we check if any person in the group has posted to the comment. If we find one, the comment is treated as if it was posted from someone in the group.</inlineHelpText>
        <label>Extend Enable Group to Non-Members</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>GroupName__c</fullName>
        <deprecated>false</deprecated>
        <description>If enable group is checked you must specify a group name here.</description>
        <externalId>false</externalId>
        <label>GroupName</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Include_File_Attachments__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>When checked, file attachments from Chatter posts will be included in compliance email (if uploaded to Chatter post by user)</inlineHelpText>
        <label>Include File Attachments</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Include_Full_Conversation_Thread__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Include the original post and all the comments in the journal email each time someone adds a comments</description>
        <externalId>false</externalId>
        <label>Include Full Conversation Thread?</label>
        <type>Checkbox</type>
    </fields>
