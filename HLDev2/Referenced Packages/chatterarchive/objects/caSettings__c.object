        <externalId>false</externalId>
        <label>Delivery Mechanism</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Disable_Triggers__c</fullName>
        <deprecated>false</deprecated>
        <description>A comma separated string of triggers that are disabled for the instance.  Supported triggers: FeedCommentAfter, FeedItemAfter, UserStatusBefore</description>
        <externalId>false</externalId>
        <label>Disable Triggers</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
