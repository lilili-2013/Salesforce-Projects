        <externalId>false</externalId>
        <label>Subject Suffix</label>
        <length>30</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Write_to_Custom_Object__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If checked, Archive for Chatter will write a copy of each chatter post/comment to custom SFDC object for reporting purposes.</inlineHelpText>
        <label>Write to Custom Object?</label>
        <type>Checkbox</type>
