		<apex:sectionHeader title="Tenant" subtitle="{!Account.Name}" />
	</div>
	
	<!-- THS FLOW BUTTONS -->
    <c:HighStreetFlowComponent HighStreetController="{!HighStreetController}" currBtn="locrequests" />
	
    <!-- Many to Many page -->
    <c:M2M2_Component M2M2_Controller="{!controller}" M2M2_configName="TenantLocationRequest" M2M2_fromId="{!Account.Id}" />
    
</apex:page>