                function fomartTimeShow(h_24,min) {
                    var h = h_24 % 12;
                    if (h === 0) h = 12;
                    return (h < 10 ? "0" + h : h) + ":"+ min + (h_24 < 12 ? ' AM' : ' PM');
                }

                function formatToTime(dateTimeStr){
                   if(dateTimeStr != ''){
                    var timeWithAMPM = fomartTimeShow(dateTimeStr.substring(0,2),dateTimeStr.substring(3,5));
                    var timeZone1 = dateTimeStr.substring(dateTimeStr.indexOf('(')+1,dateTimeStr.indexOf(')'));
                    var timeZone;
                    if(timeZone1.indexOf(' ') != -1){
                      timeZone1 = timeZone1.split(' ');
                      timeZone = timeZone1[0][0] + timeZone1[1][0] + timeZone1[2][0];
                    }else{
                      timeZone = timeZone1;
                    }
                    return (timeWithAMPM + " " + timeZone);
                  }
                }
                
                function goToNextPrev(agendaItem, isNext){
                   var nxtElement;
                   if(isNext){
                     nxtElement = agendaItem.next().next();
                   }else{
                     nxtElement = agendaItem.prev().prev();
                   }
                   
                   if(nxtElement.attr('id')){
                     var j = nxtElement.html().lastIndexOf('<strong>Feedback</strong>');
                     if(j == -1 ){
                        nxtElement = goToNextPrev(nxtElement, isNext);
                     }
                   }
                   return nxtElement;                           
               }
