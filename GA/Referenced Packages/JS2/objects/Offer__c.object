    <webLinks>
        <fullName>New_Offer</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Offer</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/apex.js&quot;)} 



var sObject = &apos;ts2__Offer__c&apos;;
var Job_sObject = &apos;ts2__Job__c&apos;;
var sObjectType = &quot;{!$ObjectType.Offer__c}&quot;;
var JobId = &quot;{!Job__c.Id}&quot;;
var stdNewURL = &quot;{!URLFOR( $Action.Offer__c.New)}&quot;;
var URL;
var RefJobId;

if (JobId.length&gt;0) {
//Check Related List
var oP = document.getElementsByName(element.name)[0].parentNode.parentNode.childNodes[0].childNodes;
 for (var i = 0; i &lt; oP.length; i++ ) { 
  var FName = oP[i].id;
if ( FName !=null &amp;&amp; FName!=&apos;&apos; &amp;&amp; FName.indexOf(JobId)!=-1) RefJobId=FName.substr(JobId.length+1,JobId.length);
}

 var jobQuery = sforce.connection.query(&quot;select RecordType.DeveloperName,Name from &quot;+Job_sObject+&quot; where Id = &apos;&quot; +JobId+ &quot;&apos;&quot;);
 var jobQueryResult = jobQuery.getArray(&apos;records&apos;)[0];
 var recordTypeJob = jobQueryResult.RecordType.DeveloperName;
 var JobTitle = jobQueryResult.Name;

//Get Record Type ID
 var typeQuery = sforce.connection.query(&quot;select RecordType.Id from RecordType where SobjectType = &apos;&quot; + sObject + &quot;&apos; AND  RecordType.DeveloperName = &apos;&quot; + recordTypeJob + &quot;&apos;&quot;);
 var typeQueryResult = typeQuery.getArray(&apos;records&apos;);

if (typeQueryResult[0]!=null &amp;&amp; typeQueryResult[0]!=undefined) {
URL = &quot;/&quot;+sObjectType+&quot;/e?retURL=/&quot;+JobId+&quot;&amp;RecordType=&quot;+typeQueryResult[0].Id;
} else {
URL = stdNewURL;
}
URL += &quot;&amp;CF&quot;+RefJobId+&quot;=&quot;+JobTitle+&quot;&amp;CF&quot;+RefJobId+&quot;_lkid=&quot;+JobId;
}
else {
 URL = stdNewURL;
}

navigateToUrl(URL,&apos;RELATED_LIST&apos;,&apos;new&apos;+RefJobId);</url>
