<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Group of settings that help control which Short List features are available to the end 
user.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Default_Staffing_Review__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>When this setting is deselected the system will default the Review Mode to 
Standard.  When selected, the system will default the Review Mode to Staffing.  This setting is only 
used when Review Mode = 3.</description>
        <externalId>false</externalId>
        <inlineHelpText>When this setting is deselected the system will default the Review Mode to 
Standard.  When selected, the system will default the Review Mode to Staffing.  This setting is only 
used when Review Mode = 3.</inlineHelpText>
        <label>Default Staffing Review</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Add_Skills__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Add Skills button is enabled to be used from Short Lists</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Add Skills button is enabled to be used from Short Lists</inlineHelpText>
        <label>Enable Add Skills</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Add_Tags__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Apply to Jobs button is enabled to be 
used from Short Lists.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Apply to Jobs button is enabled to be 
used from Short Lists.</inlineHelpText>
        <label>Enable Add Tags</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Add_to_List__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Add to Another list button is enabled to 
be used from Short Lists.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Add to Another list button is enabled to 
be used from Short Lists.</inlineHelpText>
        <label>Enable Add to List</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Apply_to_Jobs__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Apply to Jobs button is enabled to be 
used from Short Lists.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Apply to Jobs button is enabled to be 
used from Short Lists.</inlineHelpText>
        <label>Enable Apply to Jobs</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Review_Email_Edits__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>When selected, users have the ability to send a custom email with no template 
or they can select a template and then alter the message prior to sending.  When not selected, the 
user can only select an email template without making any adjustments prior to sending.</description>
        <externalId>false</externalId>
        <inlineHelpText>When selected, users have the ability to send a custom email with no template 
or they can select a template and then alter the message prior to sending.  When not selected, the 
user can only select an email template without making any adjustments.</inlineHelpText>
        <label>Enable Review Email Edits</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Review_Email__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Review Email button is enabled to be 
used from Short Lists.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Review Email button is enabled to be 
used from Short Lists.</inlineHelpText>
        <label>Enable Review Email</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Enable_Send_Email__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>This setting is used to determine if the Send Email button is enabled to be used 
from Short Lists.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to determine if the Send Email button is enabled to be used 
from Short Lists.</inlineHelpText>
        <label>Enable Send Email</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Review_Contact_Share__c</fullName>
        <deprecated>false</deprecated>
        <description>-NONE
-READ ONLY
-READ/WRITE</description>
        <externalId>false</externalId>
        <inlineHelpText>-NONE
-READ ONLY
-READ/WRITE</inlineHelpText>
        <label>Review Contact Share</label>
        <length>10</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Review_Email_Send_Options__c</fullName>
        <deprecated>false</deprecated>
        <description>Enables you to determine if the Review feature can be used with User, Contacts, or both.</description>
        <externalId>false</externalId>
        <inlineHelpText>Internal will allow to be sent to User&apos;s only, External will allow to be sent to Contacts, Both will allow either</inlineHelpText>
        <label>Review Email Send Options</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Review_Mode__c</fullName>
        <deprecated>false</deprecated>
        <description>This setting is used to alter the Review Feature into one of three Modes.  Option 
1 uses Standard Review only.  Option 2 uses Staffing Review only.  Option 3 uses both Standard 
and Staffing review modes.</description>
        <externalId>false</externalId>
        <inlineHelpText>This setting is used to alter the Review Feature into one of three Modes.  Option 
1 uses Standard Review only.  Option 2 uses Staffing Review only.  Option 3 uses both Standard 
and Staffing review modes.</inlineHelpText>
        <label>Review Mode</label>
        <length>1</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Short List</label>
</CustomObject>
