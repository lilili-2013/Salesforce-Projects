<apex:component controller="ts2.s_ResultItemDisp">
	<apex:attribute name="value" description="MultiPicklist Value" type="String" required="true" assignTo="{!MultiPicklistValue}" />
	<apex:attribute name="field" description="Field Name" type="String" required="true" assignTo="{!FieldName}" />

	<apex:repeat var="val" value="{!values}">
		<apex:outputText value="{!val}" /><br/>
	</apex:repeat>

</apex:component>