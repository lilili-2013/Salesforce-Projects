<apex:page standardController="Account" extensions="AccountFundSummary2" readOnly="true">
    <apex:includeScript value="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js" />

    <apex:form >
       
        <style type="text/css">

  table.t1 {
    margin: 1em auto;
    border-collapse: collapse;
    font-family: Arial, Helvetica, sans-serif;
    table-layout: fixed;
    width:80%;
  }
  .t1 th, .t1 td {
       padding: 4px 8px;
       width: 110px;
  }
  .t1 thead th {
    background: #4f81bd;
    text-transform: auto;
    text-align: center;
    font-size: 15px;
    color: #fff;
  }
  .t1 tr {
    border: 1px solid #95b3d7;
  }
  .t1 tbody tr {
    border: 1px solid #95b3d7;
  }
  .t1 tbody tr:nth-child(odd) {
    background: #dbe5f0;
            text-align: center;
  }
  .t1 tbody th, .t1 tbody tr:nth-child(even) td {
    border: 1px solid #95b3d7;
            text-align: center;
  }
  .t1 tfoot th {
    background: #4f81bd;
    text-align: left;
    font-weight: normal;
    font-size: 10px;
    color: #fff;
  }
  .t1 tr *:nth-child(n), .t1 tr *:nth-child(4) {
    text-align:cente;
    border: 1px solid #95b3d7;
  }
</style>
  <table class="t1" >
        <thead>
          <tr>
              <th></th>
               <apex:repeat value="{!results2}" var="fund" rows="1">
                    <apex:repeat value="{!results2[fund]}" var="year"  first="0" >
                        <th>
                             <script type="text/javascript">
                             	var d1 = new Date();
                             	var curYear = d1.getFullYear();
                            	var firstYear = d1.getFullYear() - 6;
                            	if( {!year} == curYear)
                                	{ document.write( "YTD"); }
                            	else if( {!year} == 3000)
                                	{ document.write( "Totals"); }
                            	else if( {!year} == curYear - 6)
                                	{ document.write( "Upto " + firstYear ); }
                            	else{
                                	document.write({!year});
                                	}
                            </script>
                        </th>
                   	</apex:repeat>
                 </apex:repeat>
               </tr>
            </thead>
            
                  <apex:repeat value="{!results2}" var="fund">                     
                      <tr> 
                         <th>{!fund}</th>
                      		<apex:repeat value="{!results2[fund]}" var="year" >
                              <td>
                                 $ {!MapResults[fund][year]}
                              </td>
                 			</apex:repeat>
              		 </tr>
            	 </apex:repeat> 
      
      
      
          </table> 
    </apex:form>
</apex:page>