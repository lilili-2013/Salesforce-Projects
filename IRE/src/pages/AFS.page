<apex:page standardController="Account" extensions="AFS" readOnly="true">
      
   <apex:form >
        
        <style type="text/css">

  table.t1 {
    margin: 1em auto;
    border-collapse: collapse;
    font-family: Arial, Helvetica, sans-serif;
    table-layout: fixed;
    width:80%;
  }
  .t1 th, .t1 td {
       padding: 4px 8px;
       width: 110px;
  }
  .t1 thead th {
    background: #4f81bd;
    text-transform: auto;
    text-align: center;
    font-size: 15px;
    color: #fff;
  }
  .t1 tr {
    border: 1px solid #95b3d7;
  }
  .t1 tbody tr {
    border: 1px solid #95b3d7;
  }
  .t1 tbody tr:nth-child(odd) {
    background: #dbe5f0;
            text-align: center;
  }
  .t1 tbody th, .t1 tbody tr:nth-child(even) td {
    border: 1px solid #95b3d7;
            text-align: center;
  }
  .t1 tfoot th {
    background: #4f81bd;
    text-align: left;
    font-weight: normal;
    font-size: 10px;
    color: #fff;
  }
  .t1 tr *:nth-child(n), .t1 tr *:nth-child(4) {
    text-align:cente;
    border: 1px solid #95b3d7;
  }

</style>
     
     <div ng-app="ngApp">
     
          <table class="t1" >
             <thead>
          <tr>
              <th></th>
               <apex:repeat value="{!results2}" var="fund" rows="1">
                    <apex:repeat value="{!results2[fund]}" var="year"  first="0"  >
                        <th>
                             <script type="text/javascript">
                             var d1 = new Date();
                             var curYear = d1.getFullYear();
                            var firstYear = d1.getFullYear() - 6;
                            if( {!year} == 1)
                                { document.write( "YTD"); }
                            else if( {!year} == 0)
                                { document.write( "Totals"); }
                            else if( {!year} == 7)
                                { document.write(firstYear + " & Prior" ); }
                            else if ( {!year} == 2)
                                { document.write(curYear - 1 ); }
                            else if ( {!year} == 3)
                                { document.write(curYear - 2 ); }    
                            else if ( {!year} == 4)
                                { document.write(curYear - 3 ); } 
                            else if ( {!year} == 5)
                                { document.write(curYear - 4 ); }
                            else if ( {!year} == 6)
                                { document.write(curYear - 5 ); }
                            
                                
                            </script>
                            </th>
                    </apex:repeat>
                 </apex:repeat>
                 </tr>
                 </thead>
             
                 
