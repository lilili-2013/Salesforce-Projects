<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Stores ZenKraft Shipmate app custom settings for general settings. There should only be one record max per org. If utilized, the record must be created manually and the fields must be set manually.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>AllowEditOfUsedCommercialInvoices__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If Checked, user will be allowed to edit UPS Commercial Invoice records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</description>
        <externalId>false</externalId>
        <inlineHelpText>If Checked, user will be allowed to edit UPS Commercial Invoice records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</inlineHelpText>
        <label>Allow Edit of Used Commercial Invoices</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>AllowEditOfUsedIntlCommodity__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If Checked, user will be allowed to edit UPS International Commodity records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</description>
        <externalId>false</externalId>
        <inlineHelpText>If Checked, user will be allowed to edit UPS International Commodity records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</inlineHelpText>
        <label>Allow Edit of Used Intl Commodity</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>AllowEditOfUsedNaftaCo__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If Checked, user will be allowed to edit UPS NAFTA Certificate of Origins records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</description>
        <externalId>false</externalId>
        <inlineHelpText>If Checked, user will be allowed to edit UPS NAFTA Certificate of Origins records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</inlineHelpText>
        <label>Allow Edit of Used NAFTA CO</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>AllowEditOfUsedSedForms__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If Checked, user will be allowed to edit UPS Shipper Export Declarations records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</description>
        <externalId>false</externalId>
        <inlineHelpText>If Checked, user will be allowed to edit UPS Shipper Export Declarations records that have already been used in a Shipment. Any changes made will no longer reflect the data submitted to UPS with the existing shipments.</inlineHelpText>
        <label>Allow Edit of Used SED Forms</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>HideYourUPSAccountNumberInWizard__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Hide Your UPS Account Number In Wizard</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>IgnoreMaxJobLimitForScheduledTrack__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <label>Ignore Max Job Limit for Scheduled Track</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>RunDemoMode__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Configures ZenKraft Shipmate to communicate to the UPS test servers. A valid UPS Account number that is compatible with the UPS test servers must be used.</description>
        <externalId>false</externalId>
        <inlineHelpText>Configures ZenKraft Shipmate to communicate to the UPS test servers. A valid UPS Account number that is compatible with the UPS test servers must be used.</inlineHelpText>
        <label>Run Demo Mode</label>
        <type>Checkbox</type>
    </fields>
    <label>UPS Shipmate General Settings</label>
</CustomObject>
