<apex:component controller="MetaDataSearch.SearchResultsController" id="componentID" allowDML="true">
    <head>
        <title>Metadata Search</title>
        <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
        <style type="text/css">
            body {
                font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
                font-size: 13px;
            }
            .center {
                text-align: center;
            }
            .table-bordered {
                border: 1px solid #000;
                border-collapse: collapse;
                font-size: 13px;
                width: 100%
            }
            thead>tr>th {
                vertical-align: bottom;
                border: 1px solid #000;
                border-spacing: 0;
                text-align: center;
                border-collapse: collapse;
                background: #202d79;
                color: white;
            }
            td {
                vertical-align: bottom;
                border: 1px solid #000;
                border-spacing: 0;
                border-collapse: collapse;
                padding-left:5px; 
            }
            .header>td {
                font-weight: bold;
                background: #c4c4c4;
            }
            .echoArea>td {
                padding: 10px;
            }
        </style>                
    </head>
    <apex:includeScript value="{!URLFOR($Resource.MetaDataSearch__jquery, 'js/jquery-1.7.1.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.MetaDataSearch__jquery, 'js/jquery-ui-1.8.18.custom.min.js')}" />
    <apex:includeScript value="{!URLFOR($Resource.MetaDataSearch__jquery, 'js/jquery.cookie.js')}" />
    <!-- http://www.sitepoint.com/eat-those-cookies-with-jquery/ -->
    <apex:includeScript value="{!URLFOR($Resource.MetaDataSearch__jquery, 'js/jquery.blockUI.js')}" />
    <apex:stylesheet value="{!URLFOR($Resource.MetaDataSearch__jquery, 'css/jquery-ui.css')}" />

    <style>
        body {
            font-size: 13px;
        }
        
        .vfHelpText a {
            position: relative;
        }
        
        .vfHelpText a span {
            display: none;
        }
        
        .vfHelpText a:hover span {
            display: block;
            position: absolute;
            top: 1.25em;
            padding: 2px 5px;
            left: -5em;
            width: 15em;
            z-index: 100;
            border: 1px solid orange;
            background-color: #FEFDB9;
            color: black;
            line-height: 15px;
            font-weight: !important;
        }
        
        .topcorner {
            position: absolute;
            top: 10px;
            right: 50px;
        }
        
        ul li,
        ol li {
            margin-left: 0.5em;
            position: relative;
            margin-bottom: 10px;
            color: grey;
            padding-left: 4px;
            cursor: pointer;
        }
        
        .selectedDiv {
            background-color: #C4E7F2;
            border-radius: 4px;
            -moz-border-radius: 4px;
            -webkit-border-radius: 4px;
            line-height: 23px;
            color: grey !important;
            font-weight: bold;
            padding-left: 4px;
            border-bottom: 1px solid #D2D2D2;
            border-top: 1px solid #D2D2D2;
            border-left: 1px solid #D2D2D2;
            border-right: 1px solid #D2D2D2;
            border-width: 1px 1px 1px 1px;
            border-color: #D2D2D2;
        }
        
        .pgImg {
            vertical-align: middle;
            padding-right: 6px;
        }
        
        .MyPbHeader {
            font-size: 1.2em;
            font-weight: bold;
        }
        
        .searchResultTitle {
            margin-bottom: 10px;
            background-color: #2AAAFF!important;
            color: white!important;
        }
        
        .searchTextAV {
            border: 1px solid #B6B6B6;
            padding: 3px 5px 3px 5px;
            width: 280px;
            font-size: 100%;
            -moz-border-radius: 5px;
            -webkit-border-radius: 5px;
            -moz-box-shadow: 0 1px 1px #969696 inset;
            height: 16px;
        }
        
        .sidebarLinkWrap {
            display: none;
        }
        
        .resultsPresent {
            font-weight: bold;
        }
        
        .resultBlock {
            display: none;
        }
        
        .oMsg {
            padding: 11px 24px 10px 64px;
            margin-bottom: 10px;
            -webkit-border-radius: 10px;
            -moz-border-radius: 10px;
            -ms-border-radius: 10px;
            -o-border-radius: 10px;
            border-radius: 10px;
            font-size: 15px;
            line-height: 26px;
            border: 3px solid #fff;
            position: relative;
        }
        
        .oMsg:before {
            content: " ";
            position: absolute;
            left: -3px;
            top: -3px;
            bottom: -3px;
            width: 50px;
            -webkit-border-radius: 10px 0 0 10px;
            -moz-border-radius: 10px 0 0 10px;
            -ms-border-radius: 10px 0 0 10px;
            -o-border-radius: 10px 0 0 10px;
            border-radius: 10px 0 0 10px;
        }
        
        .oMsg:after {
            content: " ";
            position: absolute;
            left: 18px;
            top: 50%;
        }
        
        article {
            display: block;
        }
        
        .oMsgInfo {
            border-color: #1cb8ee;
        }
        
        .oMsgInfo:before {
            background: #1cb8ee;
        }
        
        .oMsgInfo:after {
            background-image: url(data:image/svg+xml;base64,);
            display: inline-block;
            width: 4px;
            height: 19px;
            background: transparent;
            background-position: 100%;
            background-size: 4px 19px;
            padding: 7px 0 0 2px;
            background-repeat: no-repeat;
            margin-top: -12px;
        }
        
        .oMsg>.oBtnCloseSmall {
            position: absolute;
            right: 10px;
            top: 10px;
        }
        
        .oBtnCloseSmall {
            display: inline-block;
            width: 9px;
            height: 9px;
            font-size: 17px;
            font-family: monospace;
            font-weight: 700;
            color: #949494;
            background: url(data:image/svg+xml;base64,) 50% center no-repeat;
            margin-right: 7px;
        }
        
        a {
            cursor: pointer;
            color: #0093f0;
        }
        
        #messageheader {
            box-sizing: border-box;
            color: rgb(37, 37, 35);
            display: block;
            font-family: sans-serif;
            font-size: 13px;
            height: 65px;
            line-height: 18px;
            width: 1030px;
            margin: 0 auto;
        }
    </style>
    <style>
        .overlayDiv {
            width: 100%;
            height: 100%;
            background: #000;
            filter: alpha(opacity=70);
            opacity: 0.7;
            top: 0px;
            left: 0px;
            position: fixed;
            z-index: 500;
        }
        
        .labelSpan {
            float: left;
            margin-left: 10px;
            font-size: 14px;
            font-weight: bold;
            color: #333;
        }
        
        body .pbBody table.list tr.headerRow td,
        body .pbBody table.list tr.headerRow th {
            text-align: left;
        }
    </style>
    <!-- <div class= "topcorner"><a href="mailto:support@satrangtech.com">Support</a></div>      -->
    <apex:form id="form1">
        <table width="100%">
            <tr>
                <td width="100%" style="vertical-align:top">
                    <apex:outputPanel rendered="{!!ISNULL(searchString)}">
                        <br/>
                        <div class="printheader">
                            <img src="http://satrangtech.com/metadatasearch/logo.png"/>&nbsp;
                            <div style="vertical-align:middle;display:inline-flex;position: absolute;margin-top: 10px;"><h1 class="pageType noSecondHeader" style="color: #000;font-size: 1.5em;font-weight: normal;">Metadata Export {!IF(AND(searchCategory!='',searchCategory != '1'), 'of ' +searchCategory , '')} for search term : {!searchString}</h1></div>                       
                            <div class="printControls" style="float:right;margin-right:20px;margin-top:3px;">
                            <small>
                                <a href="javascript:window.close%28%29%3B">Close Window</a>
                                &nbsp;&nbsp;|&nbsp;&nbsp;
                                <a href="javascript:window.print%28%29%3B">Print This Page</a>
                            </small>
                            </div>                            
                        </div>
                        <br/>
                        <br/>
                        <apex:pageBlock title="Recent Items ({!recents.size})" tabStyle="RecentlyViewed" rendered="{!OR(searchCategory=='Recent Items',searchCategory=='1',searchCategory=='')}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/trophy24.png" styleClass="pgImg" />
                                    <apex:outputText value="Recent Items ({!recents.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!recents.size}" value="{!recents}" var="t" rendered="{!recents.size>0}">
                                <apex:column headerValue="Name" width="25%">
                                    <a href="/{!t.Id}" target="_blank">{!t.Name}</a>
                                </apex:column>
                                <apex:column value="{!t.Type}" />
                                <apex:column value="{!t.Email}" />
                                <apex:column value="{!t.IsActive}" />
                                <apex:column value="{!t.LastViewedDate}" />
                                <apex:column value="{!t.LastReferencedDate}" />
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!recents.size==0}" /> <br/>
                        </apex:pageBlock>

                        <apex:pageBlock title="Fields ({!fieldList.size})" rendered="{!AND(configManager.MetaDataSearch__SearchFields__c,OR(searchCategory=='Fields',searchCategory=='1',searchCategory==''))}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/custom51_100/redcross24.png" styleClass="pgImg" />
                                    <apex:outputText value="Fields ({!fieldList.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!fieldList.size}" value="{!fieldList}" var="wl" rendered="{!fieldList.size>0}">
                                <apex:column headerValue="Label">
                                    <apex:outputText value="{!wl.label}" />
                                </apex:column>
                                <apex:column headerValue="API Name">
                                    <apex:outputText value="{!wl.apiName}" />
                                </apex:column>
                                <apex:column headerValue="Is Custom?">
                                    <apex:image URL="{!IF(wl.iscustom,'/img/checkbox_checked.gif','/img/checkbox_unchecked.gif')}" />
                                </apex:column>
                                <apex:column headerValue="Is Formula?">
                                    <apex:image URL="{!IF(wl.isFormula,'/img/checkbox_checked.gif','/img/checkbox_unchecked.gif')}" />
                                </apex:column>
                                <apex:column headerValue="Object (API Name)">
                                    <apex:outputText value="{!wl.objectlabel} ({!wl.objectname})" />
                                </apex:column>
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!fieldList.size==0}" /> <br/>
                        </apex:pageBlock>
                        <apex:pageBlock title="Objects ({!objects.size})" rendered="{!AND(configManager.MetaDataSearch__Search_Objects__c,OR(searchCategory=='Objects',searchCategory=='1',searchCategory==''))}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/portals24.png" styleClass="pgImg" />
                                    <apex:outputText value="Objects ({!objects.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!objects.size}" value="{!objects}" var="wl" rendered="{!objects.size>0}">
                                <apex:column headerValue="Action">
                                    <a href="{!IF(wl.isCustom,'/p/setup/custent/CustomObjectsPage?setupid=CustomObjects','/ui/setup/Setup?setupid='+wl.apiName)}" target="_blank">View</a>
                                </apex:column>
                                <apex:column headerValue="Is Custom?">
                                    <apex:image URL="{!IF(wl.iscustom,'/img/checkbox_checked.gif','/img/checkbox_unchecked.gif')}" />
                                </apex:column>
                                <apex:column headerValue="Label">
                                    <apex:outputText value="{!wl.label}" />
                                </apex:column>
                                <apex:column headerValue="API Name">
                                    <apex:outputText value="{!wl.apiName}" />
                                </apex:column>
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!objects.size==0}" /><br/>

                        </apex:pageBlock>


                        <apex:pageBlock title="Workflow Rules ({!workflowRules.size})" rendered="{!AND(isAPIEnabled,configManager.MetaDataSearch__Search_Workflow_Rules__c, OR(searchCategory=='Workflow Rules',searchCategory=='1',searchCategory==''))}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/wrench24.png" styleClass="pgImg" />
                                    <apex:outputText value="Workflow Rules ({!workflowRules.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!workflowRules.size}" value="{!workflowRules}" var="t" rendered="{!workflowRules.size>0}">
                                <apex:column headerValue="Active?" width="3%">
                                    <apex:image URL="{!IF(t.Metadata.active,'/img/checkbox_checked.gif','/img/checkbox_unchecked.gif')}" />
                                </apex:column>
                                <apex:column headerValue="Object" width="10%%">
                                    {!LEFT(t.FullName,FIND('.',t.FullName)-1)}
                                </apex:column>
                                <apex:column headerValue="Name" width="27%">
                                    <a href="/{!t.Id}" target="_blank">{!RIGHT(t.FullName,LEN(t.FullName)-FIND('.',t.FullName))}</a>
                                </apex:column>
                                <apex:column headerValue="Filter / Criteria" width="30%">
                                    <apex:outputText rendered="{!NOT(ISNULL(t.Metadata.formula))}" value="{!t.Metadata.formula}" />
                                    <apex:outputText rendered="{!t.Metadata.criteriaItems.size>0}">
                                        <apex:outputText rendered="{!t.Metadata.criteriaItems.size>1}">
                                            Boolean Filter:&nbsp;{!IF(AND(ISNULL(t.metadata.booleanFilter),t.Metadata.criteriaItems.size==2),'1 AND 2',t.metadata.booleanFilter)}
                                        </apex:outputText>
                                        <ol>
                                            <apex:repeat rendered="{!t.Metadata.criteriaItems.size>1}" value="{!t.Metadata.criteriaItems}" var="ci">
                                                <li>{!ci.field}&nbsp;<b>{!ci.operation}</b>&nbsp;{!IF(ISBLANK(ci.value),'null',ci.value)}</li>
                                            </apex:repeat>
                                        </ol>
                                        <apex:repeat rendered="{!t.Metadata.criteriaItems.size==1}" value="{!t.Metadata.criteriaItems}" var="ci">
                                            {!ci.field}&nbsp;<b>{!ci.operation}</b>&nbsp;{!IF(ISBLANK(ci.value),'null',ci.value)}
                                        </apex:repeat>
                                    </apex:outputText>
                                </apex:column>
                                <apex:column headerValue="Actions" width="40%">
                                    <apex:outputText rendered="{!t.Metadata.actions.size>0}">
                                        <b>Immediate Actions:</b>
                                        <br/>
                                    </apex:outputText>
                                    <ul>
                                        <apex:repeat rendered="{!t.Metadata.actions.size>0}" value="{!t.Metadata.actions}" var="a">
                                            <li>{!a.name}&nbsp;({!a.type})</li>
                                        </apex:repeat>
                                    </ul>
                                    <apex:outputText rendered="{!t.Metadata.workflowTimeTriggers.size>0}">
                                        <b>Time-Dependent Actions:</b>
                                        <br/>
                                        <br/>
                                    </apex:outputText>
                                    <apex:repeat rendered="{!t.Metadata.workflowTimeTriggers.size>0}" value="{!t.Metadata.workflowTimeTriggers}" var="wtt">
                                        {!IF(VALUE(wtt.timeLength)>0,'After','Before')}&nbsp;{!ABS(VALUE(wtt.timeLength))}&nbsp;{!wtt.workflowTimeTriggerUnit}&nbsp;from&nbsp;{!IF(ISNULL(wtt.offsetFromField),'Rule Trigger Date',wtt.offsetFromField)}
                                        <br/>
                                        <ul>
                                            <apex:repeat rendered="{!wtt.actions.size>0}" value="{!wtt.actions}" var="a">
                                                <li>{!a.name}&nbsp;({!a.type})</li>
                                            </apex:repeat>
                                            <li>
                                                <apex:outputText rendered="{!wtt.actions.size==0}" value="No workflow actions have been added to this time trigger." />
                                            </li>
                                        </ul>

                                    </apex:repeat>
                                </apex:column>
                                <!--   
                                    <apex:column headerValue="Time Triggers" width="25%">
                                        <apex:repeat rendered="{!t.Metadata.workflowTimeTriggers.size>0}" value="{!t.Metadata.workflowTimeTriggers}" var="wtt">
                                            {!IF(VALUE(wtt.timeLength)>0,'After','Before')}&nbsp;{!ABS(VALUE(wtt.timeLength))}&nbsp;{!wtt.workflowTimeTriggerUnit}&nbsp;from&nbsp;{!IF(ISNULL(wtt.offsetFromField),'Rule Trigger Date',wtt.offsetFromField)}<br/>
                                            <ul>
                                                <apex:repeat rendered="{!wtt.actions.size>0}" value="{!wtt.actions}" var="a">
                                                    <li>{!a.name}&nbsp;({!a.type})</li>
                                                </apex:repeat>
                                            </ul>                                           
                                        </apex:repeat>                                      
                                    </apex:column>
                                    <apex:column headerValue="Evaluation Criteria" width="10%">
                                        <apex:outputText value="{!t.Metadata.triggerType}"/>
                                    </apex:column>  
                                    <apex:column headervalue="Last Modified Date" width="15%">
                                        <apex:outputText value="{0,date,M/d/yyyy h:mm a}">
                                            <apex:param value="{!t.lastModifiedDate}" /> 
                                        </apex:outputText>
                                    </apex:column>
                                    -->
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!workflowRules.size==0}" /><br/>
                        </apex:pageBlock>



                        <apex:pageBlock title="Validation Rules ({!validationRules.size})" rendered="{!AND(isAPIEnabled,configManager.MetaDataSearch__Search_Validation_Rules__c,OR(searchCategory=='Validation Rules',searchCategory=='1',searchCategory==''))}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/msg_icons/error24.png" styleClass="pgImg" />
                                    <apex:outputText value="Validation Rules ({!validationRules.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!validationRules.size}" value="{!validationRules}" var="t" rendered="{!validationRules.size>0}">
                                <apex:column headerValue="Active?" width="3%">
                                    <apex:image URL="{!IF(t.Metadata.active,'/img/checkbox_checked.gif','/img/checkbox_unchecked.gif')}" />
                                </apex:column>
                                <apex:column headerValue="Object" width="10%%">
                                    {!LEFT(t.FullName,FIND('.',t.FullName)-1)}
                                </apex:column>
                                <apex:column headerValue="Name" width="27%">
                                    <a href="/{!t.Id}" target="_blank">{!RIGHT(t.FullName,LEN(t.FullName)-FIND('.',t.FullName))}</a>
                                </apex:column>
                                <apex:column headerValue="Description" width="20%">
                                    {!t.Metadata.description}
                                </apex:column>
                                <apex:column headerValue="Error Condition Formula" width="20%">
                                    {!t.Metadata.errorConditionFormula}
                                </apex:column>
                                <apex:column headerValue="Error Message" width="20%">
                                    {!t.Metadata.errorMessage}
                                </apex:column>
                                <!-- 
                                    <apex:column headervalue="Last Modified Date" width="10%">
                                        <apex:outputText value="{0,date,M/d/yyyy h:mm a}">
                                            <apex:param value="{!t.lastModifiedDate}" /> 
                                        </apex:outputText>
                                    </apex:column>   
                                    -->
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!validationRules.size==0}" /> <br/>

                        </apex:pageBlock>

                        <apex:pageBlock title="Pages ({!pages.size})" tabStyle="ApexPage" rendered="{!OR(searchCategory=='Pages',searchCategory=='1',searchCategory=='')}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/service_contracts24.png" styleClass="pgImg" />
                                    <apex:outputText value="Pages ({!pages.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!pages.size}" value="{!pages}" var="p" rendered="{!pages.size>0}">
                                <apex:column headerValue="Name" width="20%">
                                    <a href="/{!p.Id}" target="_blank">{!p.Name}</a>
                                </apex:column>
                                <apex:column headerValue="Label" width="20%">
                                    <a href="/{!p.Id}" target="_blank">{!p.MasterLabel}</a>
                                </apex:column>
                                <apex:column value="{!p.NamespacePrefix}" rendered="{!configManager.MetaDataSearch__Search_Managed_Pages__c}" />
                                <apex:column value="{!p.CreatedDate}" />
                                <apex:column value="{!p.CreatedById}" />
                                <apex:column value="{!p.LastModifiedDate}" />
                                <apex:column value="{!p.LastModifiedById}" />
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!pages.size==0}" /><br/>
                        </apex:pageBlock>

                        <apex:pageBlock title="Apex Classes ({!classes.size})" rendered="{!OR(searchCategory=='Apex Classes',searchCategory=='1',searchCategory=='')}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/custom51_100/gears24.png" styleClass="pgImg" />
                                    <apex:outputText value="Apex Classes ({!classes.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!classes.size}" value="{!classes}" var="c" rendered="{!classes.size>0}">
                                <apex:column headerValue="Name" width="25%">
                                    <a href="/{!c.Id}" target="_blank">{!c.Name}</a>
                                </apex:column>
                                <apex:column value="{!c.NamespacePrefix}" rendered="{!configManager.MetaDataSearch__SearchManagedClasses__c}" />
                                <apex:column value="{!c.CreatedDate}" />
                                <apex:column value="{!c.CreatedById}" />
                                <apex:column value="{!c.LastModifiedDate}" />
                                <apex:column value="{!c.LastModifiedById}" />
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!classes.size==0}" /><br/>
                        </apex:pageBlock>


                        <apex:pageBlock title="Components ({!components.size})" tabStyle="ApexComponent" rendered="{!OR(searchCategory=='Components',searchCategory=='1',searchCategory=='')}">
                            <apex:facet name="header">
                                <apex:outputPanel layout="block" style="padding:4px">
                                    <apex:image value="/img/icon/custom51_100/buildingBlock24.png" styleClass="pgImg" />
                                    <apex:outputText value="Components ({!components.size})" styleClass="MyPbHeader" />
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:pageBlockTable rows="{!components.size}" value="{!components}" var="c" rendered="{!components.size>0}">
                                <apex:column headerValue="Name" width="20%">
                                    <a href="/{!c.Id}" target="_blank">{!c.Name}</a>
                                </apex:column>
                                <apex:column headerValue="Label" width="20%">
                                    <a href="/{!c.Id}" target="_blank">{!c.MasterLabel}</a>
                                </apex:column>
                                <apex:column value="{!c.NamespacePrefix}" rendered="{!configManager.MetaDataSearch__SearchManagedComponents__c}" />
                                <apex:column value="{!c.CreatedDate}" />
                                <apex:column value="{!c.CreatedById}" />
                                <apex:column value="{!c.LastModifiedDate}" />
                                <apex:column value="{!c.LastModifiedById}" />
                            </apex:pageBlockTable>
                            <apex:outputText value="{!NoMatchesFound}" rendered="{!components.size==0}" /><br/>
