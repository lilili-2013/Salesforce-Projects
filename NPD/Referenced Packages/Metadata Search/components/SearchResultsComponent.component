                <table id="customSettingTable">
                    <tr>
                        <td><apex:outputLabel style="font-weight:bold;" value="Query Limit For Body Elements"></apex:outputLabel></td>
                        <td>
                            <apex:inputField required="true" value="{!configManager.MetaDataSearch__Query_Limit_For_Body_Elements__c}" id="QueryLimitForBody"/>
                            <span class="vfHelpText">
                                <apex:outputLink value="javascript:return false;">
                                    <img src="/s.gif" alt="" class="helpOrb" />
                                    <span>This setting defines no. of Body Elements (such as Classes, Pages, Triggers, Templates etc) to be searched against. Body Elements
                                    can't be directly filtered with search text, hence specified no. of Body elements are fetched first and then search is performed
                                    against those fetched elements.</span>
                                </apex:outputLink>
                            </span>                             
                         </td>
                    </tr>
                    <tr>
                        <td><apex:outputLabel style="font-weight:bold;" value="Rows To Display"></apex:outputLabel></td>
