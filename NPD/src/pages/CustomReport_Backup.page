<script type="text/javascript">
function massCreate() {
    var massCreateUrl = '/apex/STAGING_oppyMassAction?retUrl=' + location.href + '&actionType=create'; 
    //var massCreateUrl = 'https://c.na7.visual.force.com/apex/opptyMassAction?actionType=create';
    var windowName = 'massActionWindow';
    
    window.open(massCreateUrl,windowName);
}

function viewReport() {
    var hasParam = 0;
//    var reportUrl = '/' + document.getElementById('reportSelect').value;
    var reportUrl = 'https://na7.salesforce.com/' + document.getElementById('reportSelect').value;
    var windowName = 'customReportWindow';
    var selectedPA = document.getElementById('masterPASelect').value;
    var selectedAU = document.getElementById('masterAUSelect').value;
    var selectedAcctclass = document.getElementById('acctclassSelect').value;
    var selectedCountry = document.getElementById('countrySelect').value;
    
    
    if ( selectedPA != null && selectedPA != "" && selectedPA != '--None--') {
        if ( document.getElementById('reportSelect').value == "00OA0000002cIK8") {
            reportUrl = reportUrl + '?pc3=00N00000008ikJz'
            reportUrl = reportUrl + '&pn3=eq'
            reportUrl = reportUrl + '&pv3=' + selectedPA;
            hasParam = 1;
        } else {
            reportUrl = reportUrl + '?pc1=00N00000008ikJz'
            reportUrl = reportUrl + '&pn1=eq'
            reportUrl = reportUrl + '&pv1=' + selectedPA;
            hasParam = 1;
        }
    }
    if ( selectedAU != null && selectedAU != "" && selectedAU != '--None--') {
        if ( hasParam == 0 ) {
            reportUrl = reportUrl + '?';
            hasParam = 1;
        } else {
            reportUrl = reportUrl + '&';
        }
        if ( document.getElementById('reportSelect').value == "00OA0000002cIK8") {
            reportUrl = reportUrl + 'pc4=00N00000008ilJX'
            reportUrl = reportUrl + '&pn4=eq'
            reportUrl = reportUrl + '&pv4=' + selectedAU;
        } else {
            reportUrl = reportUrl + 'pc2=00N00000008ilJX'
            reportUrl = reportUrl + '&pn2=eq'
            reportUrl = reportUrl + '&pv2=' + selectedAU;
        }
    }
    if ( selectedAcctclass != null && selectedAcctclass != "" && selectedAcctclass != '--None--') {
        if ( hasParam == 0 ) {
            reportUrl = reportUrl + '?';
            hasParam = 1;
        } else {
            reportUrl = reportUrl + '&';
        }
        if ( document.getElementById('reportSelect').value == "00OA0000002cIK8") {
            reportUrl = reportUrl + 'pc5=00NA00000032psM'
            reportUrl = reportUrl + '&pn5=co'
            reportUrl = reportUrl + '&pv5=' + selectedAcctclass;
        } else if ( document.getElementById('reportSelect').value == "00OA00000047yhM") {
            reportUrl = reportUrl + 'pc4=00NA00000032psM'
            reportUrl = reportUrl + '&pn4=co'
            reportUrl = reportUrl + '&pv4=' + selectedAcctclass;
        } else {
            reportUrl = reportUrl + 'pc3=00NA00000032psM'
            reportUrl = reportUrl + '&pn3=co'
            reportUrl = reportUrl + '&pv3=' + selectedAcctclass;
        }
    }
    if ( selectedCountry != null && selectedCountry != "" && selectedCountry != '--None--') {
        if ( hasParam == 0 ) {
            reportUrl = reportUrl + '?';
            hasParam = 1;
        } else {
            reportUrl = reportUrl + '&';
        }
        if ( document.getElementById('reportSelect').value == "00OA0000002cIK8") {
            reportUrl = reportUrl + 'pc6=00N00000008imAq'
            reportUrl = reportUrl + '&pn6=eq'
            reportUrl = reportUrl + '&pv6=' + selectedCountry;
        } else if ( document.getElementById('reportSelect').value == "00OA00000047yhM") {
            reportUrl = reportUrl + 'pc5=00N00000008imAq'
            reportUrl = reportUrl + '&pn5=eq'
            reportUrl = reportUrl + '&pv5=' + selectedCountry;
        } else {
            reportUrl = reportUrl + 'pc4=00N00000008imAq'
            reportUrl = reportUrl + '&pn4=eq'
            reportUrl = reportUrl + '&pv4=' + selectedCountry;
        }
    }
    
    window.open(reportUrl,windowName);
}
