        <script>
            var objError;
            var objPayError;
            var readTimedOut;

            function redirectToTop()
            {
                window.location.href = "#top";
            }

            function callSubmit()
            {
                // Determining the Rent/Own radio inputs
                var currentAddressSelection = (document.getElementById("currentRentRadio").checked) ? 'Rent' : 'Own';
                var prevAddressSelection = (document.getElementById("prevRentRadio").checked) ? 'Rent' : 'Own';
                //var employmentSection = (document.getElementById("employedRadio").checked) ? 'Employed' : 'Self-Employed';

                //SubmitInCntrllr(currentAddressSelection, prevAddressSelection, employmentSection);
                SubmitInCntrllr(currentAddressSelection, prevAddressSelection);
            }

			function formatCurrency(doc)
			{
			    var val = document.getElementById(doc);
			    var temp = val.value;
			    var dec;
			    if(temp.indexOf('.') != -1)
			    {
			        dec = temp.substring(temp.indexOf('.'));
			        temp = temp.substring(0, temp.indexOf('.'));
			    }
			    temp = temp.replace(/,/g, "");

			    if(temp.length < 4) temp = temp;

			    if(temp.length == 4)
			       temp = temp.substring(0, 1) + ',' + temp.substring(1, 4);

				else if(temp.length == 5)
			       temp = temp.substring(0, 2) + ',' + temp.substring(2, 5);

				else if(temp.length == 6)
			       temp = temp.substring(0, 3) + ',' + temp.substring(3, 6);

				else if(temp.length == 7)
			       temp = temp.substring(0, 1) + ',' + temp.substring(1, 4) + ',' + temp.substring(4, 7);

				else if(temp.length == 8)
			       temp = temp.substring(0, 2) + ',' + temp.substring(2, 5) + ',' + temp.substring(5, 8);

				else if(temp.length == 9)
			       temp = temp.substring(0, 3) + ',' + temp.substring(3, 6) + ',' + temp.substring(6, 9);

				else if(temp.length == 10)
			       temp = temp.substring(0, 1) + ',' + temp.substring(1, 4) + ',' + temp.substring(4, 7) + ',' + temp.substring(7, 10);

				else if(temp.length == 11)
			       temp = temp.substring(0, 2) + ',' + temp.substring(2, 5) + ',' + temp.substring(5, 8) + ',' + temp.substring(8, 11);

			    else if(temp.length == 12)
			       temp = temp.substring(0, 3) + ',' + temp.substring(3, 6) + ',' + temp.substring(6, 9) + ',' + temp.substring(9, 12);

			    else if(temp.length == 13)
			       temp = temp.substring(0, 1) + ',' + temp.substring(1, 4) + ',' + temp.substring(4, 7) + ',' + temp.substring(7, 10) + ',' + temp.substring(10,13);

			    else if(temp.length == 14)
			       temp = temp.substring(0, 2) + ',' + temp.substring(2, 5) + ',' + temp.substring(5, 8) + ',' + temp.substring(8, 11) + ',' + temp.substring(11, 14);

				else if(temp.length == 15)
			       temp = temp.substring(0, 3) + ',' + temp.substring(3, 6) + ',' + temp.substring(6, 9) + ',' + temp.substring(9,12) + ',' + temp.substring(12, 15);

			   if(dec != 'undefined' && dec != null)
			   temp = temp + dec;
			   val.value = temp;
			}
			/* Funcion to change number format example depending upon PayByType (Card Type) Selected on Payment form */
			function callChangeNumberFormat16()
            {
            	document.getElementById('OPCardNumberformat15').style.display = 'none';
            	document.getElementById('OPCardNumberformat16').style.display = 'block';
            	document.getElementById('OPCardNumberformat4').style.display = 'none';
            	document.getElementById('OPCardNumberformat3').style.display = 'block';
            }
			function callChangeNumberFormat15()
            {
            	document.getElementById('OPCardNumberformat15').style.display = 'block';
            	document.getElementById('OPCardNumberformat16').style.display = 'none';
            	document.getElementById('OPCardNumberformat4').style.display = 'block';
            	document.getElementById('OPCardNumberformat3').style.display = 'none';
            }

        </script>
