<apex:page sidebar="false" showHeader="false" standardStyleSheets="false" action="{!validateFromERenter}" controller="SL_Success_Controller">

<head>
	<title>Stonehenge</title>
	<apex:stylesheet value="{!URLFOR($Resource.PaymentApp, 'css/all.css')}"/>
	<apex:includeScript value="{!URLFOR($Resource.PaymentApp, 'js/jquery-1.7.1.min.js')}"/>
	<apex:includeScript value="{!URLFOR($Resource.PaymentApp, 'js/jquery.main.js')}"/>
</head>

<body> 
	<!-- wrapper -->
	<div id="wrapper">
		<div class="wrapper-holder">
			<!-- header -->
			<div id="header">
				<!-- logo -->
				<h1 class="logo"><a href="#">Stonehenge</a></h1>
				<!-- slogan -->
				<strong class="slogan">Apartment Application. Life. In Contrast.</strong>
			</div>
			<!-- main -->
			<div id="main">
				<div class="main-holder">
					<!-- article-area -->
					<div class="article-area">
						<div class="area-content">
							<div class="heading">
								<h2>Thank you for submitting your application.</h2>
							</div>
							<p>You will be receiving an email from us with further instructions shortly.</p>
						</div>
						<!-- title-box -->
						<div class="title-box">
							<div class="holder">
								<div class="frame">
									<h2>SUCCESS!</h2>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</apex:page>