    <script type="text/javascript" >
    $('select').change(function () { 
        var id = this.id;
        if(id.indexOf('applicants') == -1)
        {
            $(this).find('option:first-child').css('display','none');
        }
    }); 
    
    function hideFirstOption()
    {
        $('[id*="state"]').find('option:first-child').css('display','none');
    }
    
    $('.btn').click(function () 
    {   
        ///Scroll up
        $('html, body').animate({  
            scrollTop: 0
        }, 'slow');
    });
    
    ///Data mask for fields which are rerendered in Personal info section
    function AppTypeDataMask()
    {
        $('[id$="ss"]').mask('999-99-9999');
        $('[id$="dob"]').mask('99/99/9999');
        $('[id$="TAX"]').mask('999-99-9999');
        $('[id$="DATEOFINCORPORATION"]').mask('99/99/9999');
    }
    
    ///Data mask for fields which are rerendered in Current address section
    function CurrentAddDataMask()
    {
        $('[id$="end"]').mask('99/99/9999');
        $('[id$="tel"]').mask('999-999-9999');
    }
    
    ///Data mask for fields which are rerendered in other applicant section
    function OtherAppDataMask()
    {
        $('[id$="cel-number"]').mask('999-999-9999');
        $('[id$="cel"]').mask('999-999-9999');
    }
    
    ///Data mask for fields which are rendered in Employment section
    function EmploymentDataMask()
    {
        $('[id$="date-of-hire"]').mask('99/99/9999');
        $('[id$="phone-number"]').mask('999-999-9999');
    }
    
    function scrollUp()
    {
        if($("[id$='hiddenField1']").val() == 'Rent')
            $('[id="radio-1"]').prop('checked', true);
        else if($("[id$='hiddenField1']").val() == 'Own')
            $('[id="radio-2"]').prop('checked', true);
        
        
        if($("[id$='hiddenField2']").val() == 'Employed')
            $('[id="radio-3"]').prop('checked', true);
        if($("[id$='hiddenField2']").val() == 'Self-Employed')
            $('[id="radio-4"]').prop('checked', true);
        if($("[id$='hiddenField2']").val() == 'Unemployed')
            $('[id="radio-5"]').prop('checked', true);
        if($("[id$='hiddenField2']").val() == 'Student')
            $('[id="radio-6"]').prop('checked', true);
        
        ///Scroll up
        $('html, body').animate({  
            scrollTop: 0
        }, 'slow');
    }
    </script>
    <script type="text/javascript" >
    function radioClicked()
    {
       if ($('[id="radio-1"]').is(':checked'))
             $("[id$='hiddenField1']").val('Rent');
       else if ($('[id="radio-2"]').is(':checked'))
              $("[id$='hiddenField1']").val('Own');
       if ($('[id="radio-3"]').is(':checked'))
             $("[id$='hiddenField2']").val('Employed');
       else if ($('[id="radio-4"]').is(':checked'))
             $("[id$='hiddenField2']").val('Self-Employed');
       else if ($('[id="radio-5"]').is(':checked'))
             $("[id$='hiddenField2']").val('Unemployed');
       else if ($('[id="radio-6"]').is(':checked')) 
             $("[id$='hiddenField2']").val('Student');
             
       if($('[id="radio-1"]').is(':checked') || $('[id="radio-2"]').is(':checked'))
           showHideRentOrOwn();
           
       if($('[id="radio-3"]').is(':checked') || $('[id="radio-4"]').is(':checked') || $('[id="radio-5"]').is(':checked') || $('[id="radio-6"]').is(':checked'))
           showHideEmploymentFields();
    }
   </script>
   
   
