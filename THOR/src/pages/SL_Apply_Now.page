<apex:page sidebar="false" showHeader="false" controller="SL_ApplicationForm_Controller" id="thePage" action="{!checkForDuplicate}" standardStylesheets="false" cache="false">
<head>
    <!-- set the encoding of your site -->
    <meta charset="utf-8" />
    <title>Thor Residential Apartment Application</title>
    <!-- include the site stylesheet -->
    <link href="{!URLFOR($Resource.SL_CssForDocUploadScreenAndApplicationForm, 'css/all.css')}" rel="stylesheet" media="all" type="text/css" />
</head>
<style>
.btn{ 
background:#f42c4d !important;
    border:none !important;
    color: #fff !important;
    font:700 14px/25px "brandon-grotesque", Arial, sans-serif !important;
    min-width: 152px !important;
    height: 49px !important;
    -webkit-border-radius: 3px !important;
    border-radius: 3px !important;
    -webkit-box-shadow: 1px 1px 1px rgba(0,0,0,.75) !important;
    box-shadow: 1px 1px 1px rgba(0,0,0,.75) !important;
    padding: 0 37px !important;
    float: right !important;
}
.rental-form em{
color: #FFFFFF;
font: 12px/14px Arial, sans-serif;
display: block;
text-align: center;
float: left;
margin-top: 2px;
}
.btn:hover{background:#c91734 !important;}
input.allIn::-webkit-input-placeholder { font-size: 18px;font-weight: 500;color: #1c1c1c; letter-spacing: -1px;}
input.allIn::-moz-placeholder { font-size: 18px;font-weight: 500;color: #1c1c1c; letter-spacing: -1px;}
input.allIn:-ms-input-placeholder { font-size: 18px;font-weight: 500;color: #1c1c1c; letter-spacing: -1px;}
input.allIn:-moz-placeholder { font-size: 18px;font-weight: 500;color: #1c1c1c; letter-spacing: -1px;}
</style>
<body>
    <!-- main container of all the page elements -->
    <div id="wrapper">
        <!-- header of the page -->
        <header id="header">
            <div class="header-holder">
                <!-- page logo -->
                <div class="logo">
                    <a href="#"><img src="{!URLFOR($Resource.SL_CssForDocUploadScreenAndApplicationForm, 'images/logo.jpg')}" alt="Thor Logo" /></a>
                </div>
            </div>
        </header>
        <!-- contain main informative part of the site -->
        <main id="main" role="main">
            <!-- info -->
            <section class="info">   
                <header class="heading"> 
                    <h1>RENTAL APPLICATION</h1> 
                    <ul class="steps">
                        <li class="active"><span>1</span></li>
                        <li><span>2</span></li>
                        <li><span>3</span></li>
                    </ul>
                </header>
                <!-- rental form -->
                <form  action="#" class="rental-form validate-form">
                    <fieldset>
                        <apex:form id="mainForm">
                        
                        <apex:actionFunction name="SubmitInCntrllr" action="{!goToPayment}" rerender="error,mainPanel" onComplete="scrollUp();"/>
                        <apex:actionFunction name="showHideRentOrOwn" action="{!showHideRentOrOwn}" rerender="LeaseEnd,LandLordPhoneAndCompany" onComplete="CurrentAddDataMask();"/>
                        <apex:actionFunction name="showHideEmploymentFields" action="{!showHideEmploymentFields}" rerender="EmploymentSection" onComplete="EmploymentDataMask();"/>
                        
                        <apex:outputPanel id="mainPanel">
                        <!-- block -->
                        <div class="block">
                            <div class="heading">
                                <h2>PERSONAL INFORMATION</h2>
                            </div>
                            <!-- block holder -->
                            <div class="block-holder">
                                <div class="row">
                                    <div class="col col2 valid-row">
                                        <span class="fake-select">
                                            <apex:selectList value="{!strAppType}" id="applicationType"  size="1" styleClass="required-select success">
                                                <apex:selectOption itemValue="" itemLabel="Application Type"/>
                                                <apex:selectOption itemValue="Individual" itemLabel="Individual"/>
                                                <apex:selectOption itemValue="Company" itemLabel="Company"/>
                                                <apex:actionSupport event="onchange" rerender="opMain" oncomplete="AppTypeDataMask();"/>
                                            </apex:selectList>
                                        </span>
                                        <span class="error-msg" >Please select the Application Type</span>
                                    </div>
                                </div>
                                
                               <apex:outputPanel id="opMain">
                                    <apex:outputPanel rendered="{!If(OR(strAppType=='Individual',strAppType==''),true,false)}">
                                        <div class="row">
                                            <div class="col col2 valid-row {!errorPI_ApplicantName}">
                                                <apex:inputText value="{!applicantName}" html-placeholder="First Name" styleClass="success required" />
                                                <span class="error-msg">Please enter your First Name</span>
                                            </div> 
                                            <div class="col col3 valid-row">
                                                <apex:inputText value="{!applicantMiddleInitial}" html-placeholder="Middle Initial" styleClass="success" />
                                                <span class="error-msg">Please enter your Middle Initial</span>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col col2 valid-row {!errorPI_applicantLastName}">
                                                <apex:inputText value="{!applicantLastName}" html-placeholder="Last Name" styleClass="success required" />
                                                <span class="error-msg">Please enter your Last Name</span>
                                            </div>
                                            <div class="col col3 valid-row">
                                                <span class="fake-select">
                                                    <apex:selectList size="1" value="{!strSuffix}"  id="suffix"  styleClass="success">
                                                        <apex:selectOptions value="{!Suffix}" />
                                                    </apex:selectList>
                                                </span>
                                                <span class="error-msg">Please select the suffix</span>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col valid-row {!errorPI_ssn}">
                                                <label for="ss" class="label label1" style="width:160px !important">Social Security #</label>
                                                 <div class="input-holder input-holder1" style="width:160px !important">
                                                    <apex:inputText value="{!ssn}" html-placeholder="000-00-0000" id="ss" html-data-mask="999-99-9999" styleClass="custom-mask success required" maxLength="11" />
                                                </div>
                                                <em>Type Social Security # 000-00-0000</em>
                                                <span class="error-msg">Please enter your Social Security</span>
                                            </div> 
                                            <div class="col valid-row {!errorPI_dob}">
                                                <label for="dob" class="label">DOB</label>    
                                                <div class="input-holder">
                                                     <apex:inputText value="{!dob}" html-placeholder="MM/DD/YYYY" id="dob" html-data-mask="99/99/9999" styleClass="custom-mask success required" maxLength="10"/>
                                                </div>
                                                <em>Type Date of Birth MM/DD/YYY</em>
                                                <span class="error-msg">Please enter your Date Of Birth</span>   
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                
                                    <apex:outputPanel rendered="{!strAppType=='Company'}">
                                        <div class="row valid-row {!errorPI_CompanyName}">
                                             <apex:inputText value="{!strCompanyName}" html-placeholder="Company Name"  styleClass="success required" />
                                            <span class="error-msg">Please enter your Company Name</span>
                                        </div>
                                        
                                        <div class="row">
                                            <div class="col valid-row {!errorPI_CompanyFirst}">
                                                <apex:inputText value="{!strCompanyFirst}" html-placeholder="Contact First Name" styleClass="success required" />
                                                <span class="error-msg">Please enter your First Name</span>
                                            </div>
                                            <div class="col valid-row {!errorPI_CompanyLast}">
                                                <apex:inputText value="{!strCompanyLast}" html-placeholder="Contact last Name" styleClass="success required" />
                                                <span class="error-msg">Please enter your Last Name</span>
                                            </div>
                                        </div>
                                        
                                        <div class="row">
                                            <div class="col valid-row {!errorPI_TaxId}">
                                                <label for="TAX" class="label label1">TaxId</label>
                                                 <div class="input-holder input-holder1">
                                                    <apex:inputText value="{!strTaxID}" html-placeholder="000-00-0000" id="TAX" html-data-mask="999-99-9999" styleClass="custom-mask success required" maxLength="11"/>
                                                </div>
                                                <em>Type TAX ID 000-00-0000</em>
                                                <span class="error-msg">Please enter your Tax ID</span>
                                            </div> 
                                            <div class="col valid-row {!errorPI_DateOfIncorporation}">
                                                <label for="DATEOFINCORPORATION" class="label" style="font-size:18px;padding-left:3px;">Incorporation</label>    
                                                <div class="input-holder">       
                                                     <apex:inputText value="{!strDateOfIncorporation}" html-placeholder="MM/DD/YYYY" id="DATEOFINCORPORATION" html-data-mask="99/99/9999" styleClass="custom-mask success required" maxLength="10"/>
                                                </div> 
                                                <em>Type Date of Incorporation MM/DD/YYY</em>
                                                <span class="error-msg">Please enter Date of Incorporation</span>   
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                                
                                
                                <div class="row">
                                    <div class="col valid-row {!errorPI_phone}">
                                        <label for="cell" class="label label1">Cell</label> 
                                        <div class="input-holder input-holder1">
                                            <apex:inputText value="{!phone}" html-placeholder="000-000-0000" id="cell" html-data-mask="999-999-9999" styleClass="custom-mask success required" maxLength="12"/>
                                        </div>
                                        <em>For International numbers please type country code</em>
                                        <span class="error-msg">For International numbers please type country code</span>
                                    </div>
                                    <div class="col valid-row {!errorPI_altPhone}">
                                        <label for="phone" class="label">Alt. Phone</label>
                                        <div class="input-holder">
                                            <apex:inputText value="{!altPhone}" html-placeholder="000-000-0000" id="phone" html-data-mask="999-999-9999" styleClass="custom-mask" maxLength="12"/>
                                        </div>
                                        <em>For International numbers please type country code</em>
                                        <span class="error-msg">For International numbers please type country code</span>
                                    </div>
                                </div>
                                <div class="row valid-row {!errorPI_strEmail}">
                                     <apex:inputText value="{!strEmail}" html-placeholder="Email Address"  styleClass="success required-email" />
                                    <span class="error-msg">Please enter your email address</span>
                                </div>
                            </div>
                        </div>
                        <!-- block -->
                        <div class="block">
                            <div class="heading">
                                <h2>CURRENT ADDRESS</h2>
                            </div>
                            <!-- block holder -->
                            <div class="block-holder">
                                <div class="row">
                                    <div class="radio-holder">
                                        <label for="radio-1">
                                            <input data-state="show" id="radio-1" name="group1" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label">Rent</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div>
                                    <div class="radio-holder">
                                        <label for="radio-2">
                                            <input data-state="hide" id="radio-2" name="group1" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label">Own</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col valid-row {!errorCA_MoveInDate}">
                                        <label for="move" class="label label3">Move-In Date</label>
                                        <div class="input-holder input-holder2">
                                             <apex:inputText value="{!moveInDate}" html-placeholder="MM/DD/YYYY" id="move" html-data-mask="99/99/9999" styleClass="custom-mask success required" maxLength="10"/>
                                        </div>
                                        <em>Type Move-In Date MM/DD/YYYY</em>
                                        <span class="error-msg">Please enter your move-in date</span>
                                    </div>
                                    <apex:outputPanel id="LeaseEnd"> 
                                        <apex:outputPanel rendered="{!isShowRentOrOwnFields}">
                                         <div class="col valid-row {!errorCA_LeaseEndDate}" style="margin-left:20px;">
                                                <label for="end" class="label label3">Lease End Date</label>
                                                <div class="input-holder input-holder2">
                                                     <apex:inputText value="{!leaseEndDate}" html-placeholder="MM/DD/YYYY" id="end" html-data-mask="99/99/9999" styleClass="second-date custom-mask" maxLength="10"/>
                                                </div>
                                                <em>Type Lease End Date MM/DD/YYYY</em>
                                                <span class="error-msg">Please enter your lease end date</span>
                                            </div>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                </div>
                                <div class="row valid-row {!errorCA_Address}">
                                    <apex:inputText value="{!strAddress}" html-placeholder="Street Address"  styleClass="success required" />
                                    <span class="error-msg">Please enter your street address</span>
                                </div>
                                <div class="row">
                                    <div class="col valid-row {!errorCA_Country}">
                                        <span class="fake-select">
                                            <apex:selectList value="{!strCountry}"  id="country"  size="1" styleClass="required-select success">
                                                <apex:actionSupport event="onchange" action="{!changeCurrentStateBasedOnCountry}" rerender="state,jQueryOP,statePicklist" />
                                                <apex:selectOptions value="{!Country}"/>
                                            </apex:selectList>
                                        </span>     
                                        <span class="error-msg">Please select your country</span>
                                    </div>
                                    <apex:outputPanel id="statePicklist">
                                        <apex:outputPanel rendered="{!isShowState}">
                                            <div class="col valid-row {!errorCA_State}" style="margin-left:20px;">
                                                <span class="fake-select">
                                                    <apex:selectList size="1" value="{!strState}" id="state" onChange="hideFirstOption();" >
                                                        <apex:selectOptions value="{!lstCurrentAddressStates}"/>
                                                    </apex:selectList>
                                                </span> 
                                                <span class="error-msg">Please select your state</span>
                                            </div>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                </div>
                                <div class="row">
                                    <div class="col valid-row {!errorCA_City}">
                                        <apex:inputText value="{!strCity}" html-placeholder="City" styleClass="success required" />
                                        <span class="error-msg">Please enter your city</span>
                                    </div>
                                    <div class="col valid-row {!errorCA_Zip}">
                                        <apex:inputText value="{!strZip}" maxlength="10"   html-placeholder="Zipcode" styleClass="success required" />
                                        <span class="error-msg">Please enter your zipcode</span>
                                    </div>
                                </div> 
                                <apex:outputPanel id="LandLordPhoneAndCompany">
                                    <apex:outputPanel rendered="{!isShowRentOrOwnFields}">
                                        <div class="row slide-row">
                                            <div class="col">
                                                <apex:inputText value="{!strCompany}" html-placeholder="Landlord/ Management Company" styleClass="success" />
                                                <span class="error-msg">Please enter your company name</span>
                                            </div>
                                            <div class="col">
                                                <label for="tel" class="label">Phone</label>
                                                <div class="input-holder">
                                                     <apex:inputText value="{!strPhone}" html-placeholder="000-000-0000" id="tel" html-data-mask="999-999-9999" styleClass="custom-mask success" maxLength="12"/>
                                                </div>
                                                <em>Type Landlord/ Management Company Phone Number</em>
                                                <span class="error-msg">Please enter your company phone number</span>
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                               </apex:outputPanel>
                            </div>
                        </div>
                        <!-- block -->
                        <div class="block">
                            <div class="heading">
                                <h2>EMPLOYMENT</h2>
                            </div>
                            <!-- block holder -->
                            <div class="block-holder">
                                <div class="row">
                                    <div class="radio-holder">
                                        <label for="radio-3">
                                            <input id="radio-3" name="group2" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label">Employed</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div>
                                    <div class="radio-holder">
                                        <label for="radio-4">
                                            <input id="radio-4" name="group2" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label" style="font-size:125%">Self-Employed</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div> 
                                    <div class="radio-holder">
                                        <label for="radio-5">
                                            <input id="radio-5" name="group2" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label">Unemployed</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div>
                                    <div class="radio-holder">
                                        <label for="radio-6">
                                            <input id="radio-6" name="group2" type="radio" onClick="radioClicked();"/>
                                            <span class="fake-input"></span>
                                            <span class="fake-label">Student</span>
                                        </label>
                                        <span class="error-msg">Please select the button</span>
                                    </div>
                                </div>
                                
                                <apex:outputPanel id="EmploymentSection" >
                                
                                    <apex:outputPanel rendered="{!isShowEmployedSection}">
                                        <div class="row">
                                            <div class="col valid-row {!errorEmp_EmploymentStartDate}">
                                                <label for="date-of-hire" class="label label3">Date Of Hire</label>
                                                <div class="input-holder input-holder2">    
                                                    <apex:inputText value="{!strEmploymentStartDate}" html-placeholder="MM/DD/YYYY" id="date-of-hire" html-data-mask="99/99/9999" styleClass="custom-mask success" maxLength="10"/>
                                                </div>
                                                <em>Type Hire Date MM/DD/YYY</em>
                                                <span class="error-msg">Please enter your date of hire</span>
                                            </div>
                                            <div class="col valid-row ">
                                                <apex:inputText value="{!strEmployer}" html-placeholder="Employer"  styleClass="success" />
                                                <span class="error-msg">Please enter employer</span>
                                            </div>
                                        </div>
                                        <div class="row valid-row">
                                            <apex:inputText value="{!strPosition}" html-placeholder="Title / Position" styleClass="success" />
                                            <span class="error-msg">Please enter your position</span>
                                        </div>
                                        <div class="row">
                                            <div class="col valid-row {!errorEmp_annualSalary}">
                                                <apex:inputText value="{!annualSalary}" maxlength="12" html-placeholder="Current Annual Salary" styleClass="success" />
                                                <span class="error-msg">Please enter your current annual salary</span>
                                            </div> 
                                            <div class="col valid-row {!errorEmp_strTotalIncome}">
                                                 <apex:inputText value="{!strTotalIncome}" maxlength="12" html-placeholder="Expected Annual Compensation(All In)" styleClass="success allIn"/>
                                                <span class="error-msg">Please enter your expected annual compensation (All In)</span>
                                            </div>
                                        </div>
                                        <div class="row valid-row">
                                            <apex:inputTextArea value="{!strOccupation}" html-placeholder="Describe what you do for a living." styleClass="success" />
                                            <span class="error-msg">Please enter describe what you do for a living.</span>
                                        </div>
                                        <div class="row">
                                            <div class="col valid-row">
                                                <apex:inputText value="{!strSupervisorName}" html-placeholder="Supervisor Name" styleClass="success " />
                                                <span class="error-msg">Please enter your supervisor name</span>
                                            </div>
                                            <div class="col valid-row {!errorEmp_SupervisorCodePhone}">
                                                <label for="phone-number" class="label">Phone</label>
                                                <div class="input-holder">
                                                    <apex:inputText value="{!strSupervisorPhone}" html-placeholder="000-000-0000" id="phone-number" html-data-mask="999-999-9999" styleClass="custom-mask success" maxLength="12"/>
                                                </div>
                                                <em>Type Supervisor Phone Number</em>
                                                <span class="error-msg">Please enter your supervisor phone number</span>
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                    
                                    <apex:outputPanel rendered="{!isShowSelfEmployedSection}">
                                         <div class="row">
                                            <div class="col valid-row ">
                                                <apex:inputText value="{!strEmployer}" html-placeholder="Employer"  styleClass="success" />
                                                <span class="error-msg">Please enter employer</span>
                                            </div>
                                            <div class="col valid-row">
                                                <apex:inputText value="{!strPosition}" html-placeholder="Title / Position" styleClass="success" />
                                                <span class="error-msg">Please enter your position</span>
                                            </div>
                                        </div>
                                        <div class="row valid-row {!errorEmp_strTotalIncome}">
                                             <apex:inputText value="{!strTotalIncome}" maxlength="12" html-placeholder="Expected Annual Compensation(All In)" styleClass="success allIn"/>
                                            <span class="error-msg">Please enter your expected annual compensation (All In)</span>
                                        </div>
                                        <div class="row valid-row">
                                            <apex:inputTextArea value="{!strOccupation}" html-placeholder="Describe what you do for a living." styleClass="success" />
                                            <span class="error-msg">Please enter describe what you do for a living.</span>
                                        </div>
                                    </apex:outputPanel>
                                    
                                    
                                    <apex:outputPanel rendered="{!isShowUnEmployedSection}">
                                        <div class="row valid-row">
                                            <apex:inputTextArea value="{!strOccupation}" html-placeholder="Describe what you do for a living." styleClass="success" />
                                            <span class="error-msg">Please enter describe what you do for a living.</span>
                                        </div>
                                    </apex:outputPanel>
                                    
                                    <apex:outputPanel rendered="{!isShowStudentSection}">
                                        <div class="row">
                                            <div class="col valid-row ">
                                                <apex:inputText value="{!strUniversity}" html-placeholder="University"  styleClass="success" />
                                            </div>
                                            <div class="col valid-row ">
                                                <apex:inputText value="{!strUniversityProgram}" html-placeholder="University Program"  styleClass="success" />
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                    
                                </apex:outputPanel>
                                
                            </div>
                        </div>
                        <!-- block -->
                        <div class="block">
                            <div class="heading">
                                <h2>OTHER INCOME SOURCE</h2>
                            </div>
                            <!-- block holder -->
                            <div class="block-holder">
                                <div class="row">
                                    <div class="col valid-row">
                                        <span class="fake-select">
                                            <apex:selectList size="1" id="incomeSource1"  value="{!strIncomeSource1}">
                                                <apex:selectOptions value="{!OtherIncomeSource1}"/>
                                            </apex:selectList>
                                        </span>
                                        <span class="error-msg">Please select your income source</span>
                                    </div>
                                    <div class="col valid-row {!errorEmp_strExpectedIncome1}">
                                        <apex:inputText maxlength="12"  value="{!strExpectedIncome1}" html-placeholder="Expected Annual Income"/>
                                        <span class="error-msg">Please enter your expected annual income</span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col valid-row">
                                        <span class="fake-select">
                                            <apex:selectList size="1" id="incomeSource2"  value="{!strIncomeSource2}">
                                                <apex:selectOptions value="{!OtherIncomeSource2}"/>
                                            </apex:selectList>
                                        </span>
                                        <span class="error-msg">Please select your income source</span>
                                    </div>
                                    <div class="col valid-row {!errorEmp_strExpectedIncome2}">
                                        <apex:inputText maxlength="12" value="{!strExpectedIncome2}" html-placeholder="Expected Annual Income" />
                                        <span class="error-msg">Please enter your expected annual income</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- block -->
                        <div class="block">
                            <div class="heading">
                                <h2>PETS</h2>
                            </div>
                            <!-- block holder -->
                            <div class="block-holder">
                                <div class="row">
                                    <div class="col">
                                        <div class="col col3 valid-row ">
                                            <apex:inputText value="{!pettype1}" html-placeholder="Pet Type"/>
                                            <span class="error-msg">Please enter your pet type</span>
                                        </div>
                                        <div class="col col3 valid-row">
                                            <apex:inputText value="{!breed1}" html-placeholder="Pet Breed"/>
                                            <span class="error-msg">Please enter your pet breed</span>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="col col3 valid-row {!errorPet_petweight1}">
                                            <apex:inputText value="{!weight1}" maxlength="3" html-placeholder="Pet Weight"/>
                                            <span class="error-msg">Please enter your pet weight</span>
                                        </div>
                                        <div class="col col3 valid-row">
                                            <apex:inputText value="{!petname1}" html-placeholder="Pet Name"/>
                                            <span class="error-msg">Please enter your pet name</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col">
                                        <div class="col col3 valid-row">
                                            <apex:inputText value="{!pettype2}" html-placeholder="Pet Type"/>
