<apex:page sidebar="false" showHeader="false" standardStyleSheets="false" action="{!validateFromERenter}" controller="SL_Success_Controller">

<head>
    <!-- set the encoding of your site -->
    <meta charset="utf-8" />
    <title>Thor Payment Screen</title>
    <!-- include the site stylesheet -->
    <link href="{!URLFOR($Resource.SL_CssForDocUploadScreenAndApplicationForm, 'css/all.css')}" rel="stylesheet" media="all" type="text/css" />
</head>

<body> 
	<!-- main container of all the page elements -->
    <div id="wrapper">
        <!-- header of the page -->
        <header id="header">
            <div class="header-holder">
                <div class="logo">
                    <a href="#"><img src="{!URLFOR($Resource.SL_CssForDocUploadScreenAndApplicationForm, 'images/logo.jpg')}" alt="Thor Logo" /></a>
                </div>
            </div>
        </header>
	   <h2 style="margin-left:500px;">Thank you for submitting your application.</h2>
	   <h3 style="margin-left:200px;">An email with further instructions will be sent shortly. In the event you have any questions, please contact your leasing agent.</h3>
   </div>
