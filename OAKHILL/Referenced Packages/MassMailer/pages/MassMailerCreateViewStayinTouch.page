<script>
$(document).ready(function(){

$('#leftclick').click(function()
{
moveleftitem();
});

$('#rightclick').click(function()
{
moverightitem();
});
$('#movetop').click(function()
{
movetotop();
});

$('#movebottom').click(function()
{
movetobottom();
});

 $('#mup').click(function(){
  moveUpItem();
 });

 $('#mdown').click(function(){
  moveDownItem();
 });

});

function movetotop(){
    $('#list1 option:selected').prependTo('#list1');
  var arr = new Array;
var sv='';
    $("#list1 option").each (function() {
       arr.push ( $(this).val() );
       sv +=$(this).val()+","; 
    });
 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;

  }
   
 function movetobottom(){
    $('#list1 option:selected').appendTo('#list1');
  var arr = new Array;
var sv='';
    $("#list1 option").each (function() {
       arr.push ( $(this).val() );
       sv +=$(this).val()+","; 
    });
 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;

  }  


function moverightitem(){  
   var arr = new Array;
   var z=$('#list option').length;
  

$('#list option:selected').each(function(){
$(this).remove();
$('#list1').append($(this));
});
var sv='';
var str = '';

    $("#list1 option").each (function() {
       arr.push ( $(this).val() );
       sv +=$(this).val()+",";       
    });
    
    
     $("#list option").each (function() {
       arr.push ( $(this).val() );
       str +=$(this).val()+",";       
    });
   

 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;
 
  document.getElementById('{!$Component.thePage.theForm.thepb.rightval}').value=str;
  
 

}

 function moveleftitem(){
  var str = '';
   var arr = new Array;



 $('#list1 option:selected').each(function(){
$(this).remove();
$('#list').append($(this));
 arr.push ( $(this).val() );
 str +=$(this).val()+",";
 });
 
 var sv='';
var str = '';
var ar=new Array();
var con=new Array();
    $("#list1 option").each (function() {     
        arr.push ( $(this).val() );   
       sv +=$(this).val()+",";       
     });
   var z='';
     $("#list option").each (function() {
     //  arr.push ( $(this).val() );
     z=$(this).val();
     if (z.indexOf("Account") >= 0) 
     {
     ar.push($(this).val());
     }
     else
     {
     con.push($(this).val());
     }
     ar.sort();
     con.sort();

       str +=$(this).val()+",";       
    });
var finallist=con.concat(ar);

$('#list option').each(function (index) {
    $(this).text(finallist[index]);
});
 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;
  document.getElementById('{!$Component.thePage.theForm.thepb.rightval}').value=str;

}

 function moveUpItem(){

  $('#list1 option:selected:first-child').prop("selected", false);
  before = $('#list1 option:selected:first').prev();
    $('#list1 option:selected').detach().insertBefore(before);
 var arr = new Array;
var sv='';
    $("#list1 option").each (function() {
       arr.push ( $(this).val() );
       sv +=$(this).val()+",";       
    });

 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;
 
 
 }

 function moveDownItem(){
 $('#list1 option:selected:last-child').prop("selected", false);
  after = $('#list1 option:selected:last').next();
    $('#list1 option:selected').detach().insertAfter(after);

  var arr = new Array;
var sv='';
    $("#list1 option").each (function() {
       arr.push ( $(this).val() );
       sv +=$(this).val()+","; 
    });
 document.getElementById('{!$Component.thePage.theForm.thepb.nam}').value=sv;


  } 

  
</script>
