<apex:component access="global">
    <apex:attribute name="componentController" description="This is typically the controller for the parent VF page" type="LStore.storedDocInline" access="global" required="true"/>
    <apex:attribute name="rowCount" description="The number of records to display per page" type="Integer" access="global" assignTo="{!componentController.requestedRowCount}" default="5" />
    <apex:attribute name="treeView" description="Display folder structure in tree-style view." type="boolean" required="false" default="true" assignTo="{!componentController.renderTreeView}"/>
    <c:storedDocListPageComponent compController="{!componentController}" />
</apex:component>