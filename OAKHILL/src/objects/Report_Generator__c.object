    <webLinks>
        <fullName>Email_Report</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Email Report</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}

if(&apos;{!Report_Generator__c.Report_Generator_Type__c}&apos; == &apos;&apos;)
{
alert(&apos;Please select report generator type.&apos;);
}
else
{
var  buttonText = this.value;
changeButtonProperties(this,true,&apos;Gray&apos;,&apos;Processing...&apos;);
var approvedStatus = &apos;Approved&apos;;
var queryResult = sforce.connection.query(&quot;SELECT COUNT() FROM Investor_Report_Document__c WHERE Report_Generator__c=&apos;&quot;+&quot;{!Report_Generator__c.Id}&quot;+&quot;&apos; AND Status__c=&apos;&quot;+approvedStatus+&quot;&apos;&quot;);
if(queryResult.size &gt; 0)
{
if(confirm(&apos;Do you want to email Documents?&apos;))
{
var result = sforce.apex.execute(&quot;SL_WS_EmailDocument&quot;,&quot;emailDocuments&quot;,
{strReportGeneratorId:&quot;{!Report_Generator__c.Id}&quot;,
reportGeneratorType : &quot;{!Report_Generator__c.Report_Generator_Type__c}&quot;});
alert(result);
changeButtonProperties(this,false,&apos;#333&apos;,buttonText);
window.location.href=window.location.href;
}
else
{
   changeButtonProperties(this,false,&apos;#333&apos;,buttonText);
}
}
else
{
   alert(&apos;Please mark atleast one document as Approved to send an email.&apos;);
   changeButtonProperties(this,false,&apos;#333&apos;,buttonText);
}
}

function changeButtonProperties(button, isDisabled, fontColor, buttonText)
{
button.disabled = isDisabled;
button.style.color = fontColor;
button.value = buttonText;
}</url>
