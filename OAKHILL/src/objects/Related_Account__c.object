    <validationRules>
        <fullName>End_Day</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
OR(
Fund__r.Name  = &quot;Oak Hill Credit Alpha Fund, L.P.&quot;,
Fund__r.Name = &quot;Oak Hill Credit Alpha Fund (Offshore), Ltd.&quot;,
Fund__r.Name = &quot;Oak Hill Credit Alpha Fund SF, L.P.&quot;,
Fund__r.Name = &quot;Oak Hill Credit Alpha Yen Feeder Fund&quot;,
Fund__r.Name = &quot;Oak Hill Credit Opportunities Fund, L.P.&quot;,
Fund__r.Name = &quot;Oak Hill Credit Opportunities Fund (Offshore), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund, L.P.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Offshore), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (EURO Feeder), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (GBP Feeder), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Yen Feeder), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (AUD Feeder), Ltd.&quot;,
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Parallel), L.P.&quot;),
((DAY(Close_Date__c) &lt;&gt; IF(Month(Close_Date__c)=12, 31, DAY(DATE(YEAR(Close_Date__c),MONTH(Close_Date__c)+1,1) 
- 1)))))</errorConditionFormula>
        <errorMessage>Date must be the last day of the month.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_More_than_One_Primary_Contact</fullName>
        <active>true</active>
        <description>This rule exists to validate that only one primary investor contact is related to a related account</description>
        <errorConditionFormula>Primary_Investor_Contacts__c &gt; 1</errorConditionFormula>
        <errorMessage>No more than one Primary Contact can be indicated per Related Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Day</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
OR( 
Fund__r.Name = &quot;Oak Hill Credit Alpha Fund, L.P.&quot;, 
Fund__r.Name = &quot;Oak Hill Credit Alpha Fund (Offshore), Ltd.&quot;, 
Fund__r.Name = &quot;Oak Hill Credit Alpha Fund SF, L.P.&quot;, 
Fund__r.Name = &quot;Oak Hill Credit Alpha Yen Feeder Fund&quot;, 
Fund__r.Name = &quot;Oak Hill Credit Opportunities Fund, L.P.&quot;, 
Fund__r.Name = &quot;Oak Hill Credit Opportunities Fund (Offshore), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund, L.P.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Offshore), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (EURO Feeder), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (GBP Feeder), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Yen Feeder), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (AUD Feeder), Ltd.&quot;, 
Fund__r.Name = &quot;OHA Diversified Credit Strategies Fund (Parallel), L.P.&quot;),  
(DAY( Open_Date__c ) &lt;&gt; 1))</errorConditionFormula>
        <errorMessage>Open day must be the first day of the month</errorMessage>
    </validationRules>
