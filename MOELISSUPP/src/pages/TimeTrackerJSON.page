<apex:page controller="TimeTrackerJSON" showHeader="false" standardStylesheets="false" contentType="text/plain" action="{!getTimeTrackers}">{
    "head" : [
        "Deal Name",
        "Client",
        "Stage",
        "Senior Banker",
        "Hours",
        "Status",
        "Notes"
    ],
    "footer" : [
    ],
    "fields" : [{
        },{
            "readonly": "true"
        },{
            "readonly": "true"
        },{
        	"validator" : {
            	"type" : "noemptyCheckHours",
            	"message" : "Please select Banker"
            }
        },{
        	"style" : "width:50px;text-align:right;"
        	,"maxLength":"10",
            "validator" : {
            	"type" : "noemptyCheckNumber",
            	"message" : "Invalid Hours"
            }
        },{
            "values" :[
                <apex:repeat value="{!TT_Status_values}" var="values" rows="1">
                    { "value" : "{!values.value}", "label" : "{!values.label}"}
                </apex:repeat>
                <apex:repeat first="1" value="{!TT_Status_values}" var="values" rows="200">
                    ,{ "value" : "{!values.value}", "label" : "{!values.label}"}
                </apex:repeat>
            ]
        },{
        "style" : "width:280px;height:33px;",
        "maxLength" : "255"
        <!-- "maxLength" : "{!fieldNoteMaxLength}" -->
        }
    ],
    "rows" : [
    <apex:repeat value="{!objList}" var="objItem" rows="1">
        "{!objItem.DealId}"
    </apex:repeat>
    <apex:repeat first="1" value="{!objList}" var="objItem" rows="200">
        ,"{!objItem.DealId}"
    </apex:repeat>
    ],
    "table" : [
    <apex:repeat value="{!objList}" var="oItem" rows="1">
        [
            {
                "value" : "{!oItem.DealName}",
                "type" : "sflink"
            },{
                "value" : "{!oItem.Client}",
                "type" : "text"
            },{
                "value" : "{!oItem.Stage}",
                "type" : "text"
            },{
                "value" : "{!oItem.sBanker}",
                "values" :[
                    { "value" : "", "label" : ""}
	                <apex:repeat value="{!oItem.listSeniorBanker}" var="values">
	                    ,{ "value" : "{!values.value}", "label" : "{!values.label}"}
	                </apex:repeat>
                ],
                "type" : "smplselectO"
            },{            
                "value" : "<apex:outputText value="{0,number,0.00}">
       						<apex:param value="{!oItem.Hours}" />
							</apex:outputText>",
                "type" : "rtextO"
            },{
                "value" : "{!oItem.Status}",
                "type" : "smplselectO"
            },{
                "value" : "{!oItem.Note}",
                "type" : "textareaO"            
            }
        ]
    </apex:repeat>
    <apex:repeat first="1" value="{!objList}" var="oItem" rows="200">
        ,[
            {
                "value" : "{!oItem.DealName}",
                "type" : "sflink"
            },{
                "value" : "{!oItem.Client}",
                "type" : "text"
            },{
                "value" : "{!oItem.Stage}",
                "type" : "text"
            },{
                "value" : "{!oItem.sBanker}",
                "values" :[
                    { "value" : "", "label" : ""}
	                <apex:repeat value="{!oItem.listSeniorBanker}" var="values">
	                    ,{ "value" : "{!values.value}", "label" : "{!values.label}"}
	                </apex:repeat>
                ],
                "type" : "smplselectO"
            },{            
                "value" : "<apex:outputText value="{0,number,0.00}">
       						<apex:param value="{!oItem.Hours}" />
							</apex:outputText>",
                "type" : "rtextO"
            },{
                "value" : "{!oItem.Status}",
                "type" : "smplselectO"
            },{
                "value" : "{!oItem.Note}",
                "type" : "textareaO"            
            }
        ]   
    </apex:repeat>
      
    ]
}

</apex:page>