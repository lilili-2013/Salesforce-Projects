<apex:page id="thePage2" showHeader="false" controller="SL_BankerActivitySummary_Dashboard" >
    <head>
        <script>
        function checkData()
        {
            var isError = false;
            var sDate = document.getElementById("thePage2:theForm2:theBlock2:theBlockS2:sDateBlockSI2:sDate2").value;
            var eDate = document.getElementById("thePage2:theForm2:theBlock2:theBlockS2:eDateBlockSI2:eDate2").value;
            
            if(sDate != '' && eDate != ''){
                if ((Date.parse(sDate) > Date.parse(eDate)) || (Date.parse(sDate) == Date.parse(eDate))){
                    alert('End Date must be later than Start Date. Both End Date and Start Date must be <= TODAY.')
                        return false;
                }
            }
            return true;
        }
        </script>	
    </head>
    <apex:form id="theForm2" target="_blank" onsubmit="return checkData();">
        <apex:pageMessages />
        <apex:pageBlock id="theBlock2"  title="{!$Label.SL_ReportSummaryName}" mode="detail">
            <apex:pageBlockSection id="theBlockS2" columns="1">
                <apex:pageBlockSectionItem id="sDateBlockSI2">
                    <apex:outputLabel value="Start Date"/>
                    <apex:inputField value="{!tmpObj.Start_Date__c}" id="sDate2"/> 
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem id="eDateBlockSI2">
                    <apex:outputLabel value="End Date"/>
                    <apex:inputField value="{!tmpObj.End_Date__c}" id="eDate2"/> 
                </apex:pageBlockSectionItem>
                <apex:commandButton action="{!run}" value="Run Report" />
            </apex:pageBlockSection> 
        </apex:pageBlock>
    </apex:form>
</apex:page>