    <apex:form >
        <div class="bRelatedList">
            <apex:pageBlock tabStyle="Account" rendered="{!IF(AccountListSize>0,true,false)}">
                <apex:facet name="header">
                    <div style="line-height:20px;">
                        <img class="relatedListIcon" alt="The alt text" src="/s.gif" />
                        <h3>Account [{!AccountListSize}]</h3>
                    </div>
                </apex:facet>
                <apex:pageBlockTable value="{!AccountList}" var="varAccount">
                    <apex:column styleClass="actionColumn">
                        <apex:facet name="header"><h3>Action</h3></apex:facet>
                            <apex:outputLink value="{!URLFOR($Action.Account.Edit, varAccount.id)}">
                                <b>Edit</b>
                            </apex:outputLink>                        
                    </apex:column>
                    <apex:column >
                        <apex:facet name="header"><h3>Name</h3></apex:facet>
                        <apex:outputLink value="{!URLFOR($Action.Account.View, varAccount.id)}">
                            {! varAccount.name}
                        </apex:outputLink>                          
                    </apex:column>
                </apex:pageBlockTable>
            </apex:pageBlock> 

            <apex:pageBlock tabStyle="Contact">
                <apex:facet name="header">
                    <div style="line-height:20px;">
                        <img class="relatedListIcon" alt="The alt text" src="/s.gif" />
                        <h3>Contact [{!ContactListSize}]</h3>
                    </div>
                </apex:facet>
                <apex:pageBlockTable value="{!ContactList}" var="varContact">
                    <apex:column value="{! varContact.name}" /> 
                </apex:pageBlockTable>
            </apex:pageBlock> 
        </div>
    </apex:form>
            
</apex:page>