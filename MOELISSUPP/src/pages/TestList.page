ccount = 0;
function onCustomLoad()
{
	
	divs = document.getElementsByTagName("*");
	
	for(var i = 0, maxI = divs.length; i < maxI; ++i)
	{
		var elm = divs[i];
		if(elm.className=="mruItem")
		{
			ccount++;
			elm.childNodes[0].href = "javascript:alert('Change link to link #"+ccount+"')";
		}
	}
	
	
	/*
