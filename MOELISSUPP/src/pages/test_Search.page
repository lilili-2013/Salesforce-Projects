<apex:page controller="test_Search">

	<apex:form >
		<apex:pageBlock title="ASC">
			<apex:pageBlockTable value="{!ascQuery}" var="q">
				<apex:column headerValue="ID">
					{!q.ID}
				</apex:column>
				<apex:column headerValue="Name">
					{!q.Name}
				</apex:column>
			</apex:pageBlockTable>
		</apex:pageBlock>
		<apex:pageBlock title="DESC">
			<apex:pageBlockTable value="{!descQuery}" var="q">
				<apex:column headerValue="ID">
					{!q.ID}
				</apex:column>
				<apex:column headerValue="Name">
					{!q.Name}
				</apex:column>
			</apex:pageBlockTable>
		</apex:pageBlock>
	</apex:form>

</apex:page>