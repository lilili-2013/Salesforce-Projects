<apex:page controller="TimeTrackerJSON_test" showHeader="false" standardStylesheets="false" contentType="text/plain">{
    "head" : [
        "Deal Name",
        "Client",
        "Hours Spent",
        "Status",
        "Notes"
    ],
    "footer" : [
    ],
    "fields" : [{
        },{
            "readonly": "true"
        },{
        },{
            "values" :[
                <apex:repeat value="{!TT_Status_values}" var="values" rows="1">
                    { "value" : "{!values.value}", "label" : "{!values.label}"}
                </apex:repeat>
                <apex:repeat first="1" value="{!TT_Status_values}" var="values" rows="200">
                    ,{ "value" : "{!values.value}", "label" : "{!values.label}"}
                </apex:repeat>
            ]
        },{
        }
    ],
    "rows" : [
    <apex:repeat value="{!objList}" var="objItem" rows="1">
        "{!objItem.Deal__c}"
    </apex:repeat>
    <apex:repeat first="1" value="{!objList}" var="objItem" rows="200">
        ,"{!objItem.Deal__c}"
    </apex:repeat>
    ],
    "table" : [
    <apex:repeat value="{!objList}" var="oItem" rows="1">
        [
            {
                "value" : {
                    "lkid" : "{!oItem.Deal__r.Client__r.Id}"
                    ,"lkold" : "{!oItem.Deal__r.Client__r.Name}"
                    ,"lktp" : "<apex:outputText value="{0}" >
								<apex:param value="{!LEFT(oItem.Deal__r.Client__r.Id,3)}"/>
								</apex:outputText>"
                    ,"formid" : "{!formId}"
            			  },
                "type" : "lookup"
            },{
                "value" : "{!oItem.Deal__r.Client__r.Name}",
                "type" : "text"
            },{            
                "value" : "<apex:outputText value="{0,number,0.00}">
       						<apex:param value="{!oItem.Hours__c}" />
							</apex:outputText>",
                "type" : "rtext"
            },{
                "value" : "{!oItem.Status__c}",
                "type" : "smplselect"
            },{
                "value" : "{!oItem.Note__c}",
                "type" : "text"            
            }
        ]
    </apex:repeat>
    <apex:repeat first="1" value="{!objList}" var="oItem" rows="200">
        ,[
            {
                "value" : {
                    "lkid" : "{!oItem.Deal__r.Client__r.Id}"
                    ,"lkold" : "{!oItem.Deal__r.Client__r.Name}"
                    ,"lktp" : "<apex:outputText value="{0}" >
								<apex:param value="{!LEFT(oItem.Deal__r.Client__r.Id,3)}"/>
								</apex:outputText>"
                    ,"formid" : "{!formId}"
            			  },
                "type" : "lookup"
            },{
                "value" : "{!oItem.Deal__r.Client__r.Name}",
                "type" : "text"
            },{
                "value" : "<apex:outputText value="{0,number,0.00}">
       						<apex:param value="{!oItem.Hours__c}" />
							</apex:outputText>",
                "type" : "rtext"
            },{
                "value" : "{!oItem.Status__c}",
                "type" : "smplselect"
            },{
                "value" : "{!oItem.Note__c}",
                "type" : "text"            
            }
        ]   
    </apex:repeat>
    ]
}

</apex:page>