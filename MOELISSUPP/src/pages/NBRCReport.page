<apex:page controller="NBRCReport" cache="true" showheader="false" contentType="{!chooserender2}" renderAs="{!chooserender}">
    <head>
        <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
        <style>
            @page {
                margin: 0.75in 0.95in 0.75in 0.75in;
                mso-header-margin: 0.0in;
                mso-footer-margin: 0.0in;
                font-family:Arial,sans-serif;
            }
    body {font-family:Arial,sans-serif;}  
    .tabl {
                background-color:white;
                border:1px solid black;
                font-size:6.0pt;
                font-family:Arial,sans-serif;
                vertical-align:top;
    }
    input{height:8px;width:8px;}
    .td_hd {
                font-weight:bold;
                vertical-align:top;
                height:0.18in;
    } 
    .td_t {
                height:0.18in;
                vertical-align:top;
                text-align:left;
    }  
    .td_t1 {
                width: 2pt;
                font-size:1.0pt;
    }  
    .head {
                background-color:rgb(68, 105, 125);
                font-size:10.0pt;
                color:white;
                font-weight:bold;
                vertical-align:center;
                height:0.18in;
    }
    .head2 {
                background-color:rgb(188, 188, 137);
                font-size:8.0pt;
                height:0.18in;
                font-weight:bold;
                text-align:left;
                vertical-align:center;
    }
    .td_t2 {
                background-color:rgb(198, 188, 137);
                width: 2pt;
                font-size:1.0pt;
    }
    /* a{font-size:6.0pt;} */
    a
    { 
        font-size:6.0pt;
        text-decoration:none; 
        font-color:black; 
        color: black;
        pointer-events: none; 
        cursor: default;
    }
    .bord{
    <apex:outputText rendered="{!IF(chooserender='pdf', 'true', 'false')}">border-top:.5pt dotted black;line-height: 0px;</apex:outputText> 
    <apex:outputText rendered="{!IF(chooserender='pdf', 'false', 'true')}">border-top:.5pt hairline black;height: 2pt;</apex:outputText>
    }
    .spacer{
           height:8px;
    }
    
    /* background-color:#44697D; RGB 68/105/125*/ 
    /*  background-color:#c6bc89; RGB 198/188/137 */ 
    /*#C6BF70;*/       
        </style>
    </head>
    <body>
        <table border="0" cellpadding="0" cellspacing="0"  class="tabl" align="center" >
         <col style="mso-width-source:userset;width: 80px;"/>
         <col style="mso-width-source:userset;width: 58px;"/>
         <col style="mso-width-source:userset;width: 6px;"/>
         <col style="mso-width-source:userset;width: 136px;"/>
         <col style="mso-width-source:userset;width: 6px;"/>
         <col style="mso-width-source:userset;width: 138px;"/>
         <col style="mso-width-source:userset;width: 6px;"/>
         <col style="mso-width-source:userset;width: 166px;"/>
            <tr><td colspan="8" style="font-size:12.0pt;font-weight:bold;">{!DealName}</td></tr>
            <!--  Start  Client Information -->
            <tr><td colspan="8" class="head">Client Information</td></tr>
            <tr>
