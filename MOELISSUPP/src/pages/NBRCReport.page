                <td class="td_t">{!dealObj.Target__c}</td>
                <td class="d_t1"></td>
                <td class="td_hd" align="right">Acquiror</td>
                <td class="d_t1"></td>
                <td class="td_t">{!dealObj.Acquiror__c}</td>
            </tr>
            <tr><td colspan="8" class="bord">&nbsp;</td></tr>
            <tr>
                <td class="td_hd" align="right" colspan="2">Target Public/Private</td>
                <td class="d_t1"></td>
                <td class="td_t">{!dealObj.Target_Public_Private__c}</td>
                <td class="d_t1"></td>
                <td class="td_hd" align="right">Acquiror Public/Private</td>
                <td class="d_t1"></td>
                <td class="td_t">{!dealObj.Acquiror_Public_Private__c}</td>
            </tr>
            <tr><td colspan="8" class="bord">&nbsp;</td></tr>
            <tr>
