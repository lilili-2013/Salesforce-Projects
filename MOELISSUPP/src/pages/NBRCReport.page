                <apex:outputText rendered="{!if(dealObj.Include_in_Pipeline__c && renderMode == 'pdf',true,false)}"><apex:image value="{!$Resource.checkbox}" /></apex:outputText>
                <apex:outputText rendered="{!if(dealObj.Include_in_Pipeline__c && renderMode == 'excel',true,false)}"><input type="checkbox" checked="true" /></apex:outputText>
                </td>
