                <td class="td_t" style="width:100px;">{!dealObj.Name}</td>
                <td class="d_t1"></td>
                <td class="td_hd" align="right">Record Type</td>
                <td class="d_t1"></td>
                <td class="td_t" style="width:100px;">{!dealObj.RecordType.Name}</td>
            </tr>
            <tr><td colspan="8" class="bord">&nbsp;</td></tr>
            <tr>
                <td class="td_hd" align="right" colspan="2">Project Code</td>
                <td class="d_t1"></td>
                <td class="td_t" >{!dealObj.Expense_Code__c}</td>
                <td class="d_t1"></td>
                <td class="td_hd" align="right">Industry</td>
                <td class="d_t1"></td>
                <td class="td_t" >{!dealObj.Industry__c}</td>
            </tr>
            <tr><td colspan="8" class="bord">&nbsp;</td></tr>
            <tr>
                <td class="td_hd" align="right" colspan="2">Company Name</td>
