            </tr>
            <tr><td colspan="8" class="bord">&nbsp;</td></tr>
            <tr>
                <td class="td_hd" align="right" colspan="2">List all bankers aware of transaction</td>
                <td class="d_t1"></td>
                <td class="td_t"   colspan="5">{!dealObj.List_all_bankers_aware_of_transaction__c}</td>
            </tr>
<!--             End  Conflict Clearance -->
        
<!--            Start Transaction Details -->
            <tr><td colspan="8" class="head">Transaction Details</td></tr>
            <tr>
                <td class="td_hd" align="right" colspan="2">Business Type</td>
                <td class="d_t1"></td>
                <td class="td_t" >{!dealObj.Business_Type__c}</td>
                <td class="d_t1"></td>
                <td class="td_hd" align="right">Stage</td>
