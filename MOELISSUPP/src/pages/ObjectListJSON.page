<apex:page controller="ObjectListJSON" showHeader="false" standardStylesheets="false" contentType="text/plain">{
<apex:outputText rendered="{!if(checkByName,true,false)}">	
"data": 
[
<apex:outputText rendered="{!if(ObjId='',true,false)}">	
	<apex:outputText rendered="{!if(ObjName=='EmployeeProfile',true,false)}">
	    <apex:repeat value="{!EmployeeProfileObjList}" var="objItem" rows="1">{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}", "additional" : "{!objItem.Title__c}"}</apex:repeat>
	    <apex:repeat value="{!EmployeeProfileObjList}" var="objItem" first="1" rows="2">,{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}, "additional" : "{!objItem.Title__c}"}</apex:repeat>
	</apex:outputText>
	<apex:outputText rendered="{!if(ObjName=='Contact',true,false)}">
	    <apex:repeat value="{!ContactObjList}" var="objItem" rows="1">{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}", "additional" : "{!objItem.Account.Name}"}</apex:repeat>
	    <apex:repeat value="{!ContactObjList}" var="objItem" first="1" rows="2">,{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}", "additional" : "{!objItem.Account.Name}"}</apex:repeat>
	</apex:outputText>
	<apex:outputText rendered="{!if(ObjName=='Account',true,false)}">
	    <apex:repeat value="{!AccountObjList}" var="objItem" rows="1">{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}"}</apex:repeat>
	    <apex:repeat value="{!AccountObjList}" var="objItem" first="1" rows="2">,{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}"}</apex:repeat>
	</apex:outputText>
</apex:outputText>
<apex:outputText rendered="{!if(ObjId='',false,true)}">
	<apex:outputText rendered="{!if(ObjName=='EmployeeProfile',true,false)}">
	    <apex:repeat value="{!EmployeeProfileByID}" var="objItem" rows="1">{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}", "additional" : "{!objItem.Title__c}"}</apex:repeat>
	</apex:outputText>
	<apex:outputText rendered="{!if(ObjName=='Contact',true,false)}">
	    <apex:repeat value="{!ContactByID}" var="objItem" rows="1">{"id" : "{!objItem.Id}", "value" : "{!objItem.Name}", "additional" : "{!objItem.Account.Name}"}</apex:repeat>
	</apex:outputText>
</apex:outputText>
]
</apex:outputText>
<apex:outputText rendered="{!if(checkByName,false,true)}">	
"list_items": 
[
	<apex:outputText rendered="{!if(ObjName=='EmployeeProfile',true,false)}">
    <apex:repeat value="{!EmployeeProfileObjList}" var="objItem" rows="1">
        {
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}",
	    	"additional" : "{!objItem.Title__c}"
		}
    </apex:repeat>
    <apex:repeat first="1" value="{!EmployeeProfileObjList}" var="objItem" rows="50">
        ,{
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}",
	    	"additional" : "{!objItem.Title__c}"
		}
    </apex:repeat>
    </apex:outputText>
    <apex:outputText rendered="{!if(ObjName=='Contact',true,false)}">
    <apex:repeat value="{!ContactObjList}" var="objItem" rows="1">
        {
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}",
	    	"additional" : "{!objItem.Account.Name}"
		}
    </apex:repeat>
    <apex:repeat first="1" value="{!ContactObjList}" var="objItem" rows="50">
        ,{
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}",
	    	"additional" : "{!objItem.Account.Name}"
		}
    </apex:repeat>
    </apex:outputText>
        <apex:outputText rendered="{!if(ObjName=='Account',true,false)}">
    <apex:repeat value="{!AccountObjList}" var="objItem" rows="1">
        {
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}"
		}
    </apex:repeat>
    <apex:repeat first="1" value="{!AccountObjList}" var="objItem" rows="50">
        ,{
	     	"key" : "{!objItem.Id}",
	    	"val" : "{!objItem.Name}"
		}
    </apex:repeat>
    </apex:outputText>
]
</apex:outputText>
}
</apex:page>