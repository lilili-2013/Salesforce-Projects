<apex:page showHeader="false" controller="ByBankerInterestReport" cache="true" renderAs="pdf" > <!--  -->
<html>
<head>
<apex:stylesheet value="{!$Resource.PDFDocumentStyle}" />
<style type="text/css">
        @page {
        	size: landscape;
        	font-fanmily: "Arial" !important;
            margin: 1cm;
            margin-bottom: 2.7cm;
            margin-top: 2cm;


            @bottom-left {
				font-size: 0.3cm;
            }

            @bottom-right {
				font-size: 0.3cm;
                content: "Page " counter(page);
            }
        }
	
</style>
</head>
	<body>
	<c:PDFHeaderFooter type="header" position="left">
		<div style="height: 28px;"></div>
		<div style="margin-left:8px; margin-right:8px;">
<!-- 			<b><apex:outputText style="font-size: 140%;" value="{curFundObj.Name}"/></b><br/>
			<b><apex:outputText style="font-size: 100%;" value="{!DateNow}"/></b><br/>
			<b>All Figures in {curFundObj.Native_Currency__c} Currency</b>
 -->		
			<b><apex:outputText style="font-size: 160%;" value="Banker Interest Report"/></b><br/>
			<b><apex:outputText style="font-size: 120%;" value="{!DateNow}"/></b><br/><br/><br/>
		</div>
	</c:PDFHeaderFooter>
	<c:PDFHeaderFooter type="header" position="center">	</c:PDFHeaderFooter>
	
	<c:PDFHeaderFooter type="header" position="left">	</c:PDFHeaderFooter>


	<c:PDFHeaderFooter type="header" position="center">
		<div style="height: 50px;"></div>
		<div style="margin-left:8px; margin-right:8px;">
		
			<table width="100%" style="collapse: collapse; border: 0px #c0c0c0 solid;" cellpadding="2" cellspacing="0">
				<tr style="background-color: #c8c8c8;">
					<td style="width:140px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>INVESTOR NAME</b></td> 
					<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>STATUS</b></td>
					<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>FUND NAME</b></td>
					<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>$ (Low, mm)</b></td>
					<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>$ (High, mm)</b></td>
					<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>Meeting</b></td>
					<td style="text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>Comments</b></td>
				</tr>		
			</table>
		</div>		
	</c:PDFHeaderFooter>



	<table width="100%" style="collapse: collapse; border: 0px #c0c0c0 solid;" cellpadding="2" cellspacing="0">
		<tr style="background-color: #c8c8c8;">
			<td style="width:140px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>INVESTOR NAME</b></td> 
			<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>STATUS</b></td>
			<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>FUND NAME</b></td>
			<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>$ (Low, mm)</b></td>
			<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>$ (High, mm)</b></td>
			<td style="width:80px; text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>Meeting</b></td>
<!-- 			<td style="text-align: center; vertical-align: top; font-size: {!FontSize};"><b>Inv Mtg</b></td>
			<td style="text-align: center; vertical-align: top; font-size: {!FontSize};"><b>DD Mtg</b></td>
 -->
			<td style="text-align: center; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>Comments</b></td>
		</tr>		
		
		
		
		<apex:repeat var="XXX" value="{!sectionList}">

	<tr style="page-break-inside: avoid;page-break-after:always;">
		<td colspan="7" style="padding: 0px;">

			<table width="100%" style="" cellpadding="0" cellspacing="0">

			<tr style="background-color: #e0e0e0;">
				<td colspan="6" style="padding:2px; text-align: left; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"><b>{!XXX.sectionName}</b></td>
				<td></td>
			</tr>
			<apex:repeat var="YYY" value="{!XXX.rows}">
				<tr>
					<td style="collapse: collapse; border-left: 1px #c0c0c0 solid;width:140px; text-align: left; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">{!YYY.investorName}</td>
					<td style="width:80px; text-align: left; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">{!YYY.status}</td>
					<td style="width:80px; text-align: left; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">{!YYY.fundName}</td>
					<td style="width:80px; text-align: right; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">
						<apex:outputText value="${0,number,#0.00}" >
							<apex:param value="{!YYY.low}" />
						</apex:outputText> 						
					</td>
					<td style="width:80px; text-align: right; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">
						<apex:outputText value="${0,number,#0.00}" >
							<apex:param value="{!YYY.high}" />
						</apex:outputText> 						
					</td>
					<td style="width:80px; text-align: right; vertical-align: top; font-size: {!FontSize}; {!BorderColors}">
						<apex:repeat var="ZZZ" value="{!YYY.invMtgDate}"> {!ZZZ}&nbsp;(Inv)<br/> </apex:repeat> 
						<apex:repeat var="ZZZ" value="{!YYY.DDMeetingDate}"> {!ZZZ}&nbsp;(DD)<br/> </apex:repeat> 
					</td>
					<td style="text-align: left; vertical-align: top; font-size: {!FontSize}; {!BorderColors}"> <apex:repeat var="ZZZ" value="{!YYY.comments}"> {!ZZZ}<br/> </apex:repeat> </td>
				</tr>
			</apex:repeat>
			
			
			<tr style="background-color: #808080;">
				<td style="padding:2px; collapse: collapse; border-left: 1px #c0c0c0 solid;text-align: left; vertical-align: top; font-size: {!FontSize}; border-top: 0px #c0c0c0 solid; border-right: 1px #808080 solid; border-bottom: 1px #c0c0c0 solid; "><b>TOTAL</b></td>
				<td style="text-align: left; vertical-align: top; font-size: {!FontSize};  border-top: 0px #c0c0c0 solid; border-left: 0px #808080 solid; border-right: 1px #808080 solid; border-bottom: 1px #c0c0c0 solid; " colspan="2"></td>
				<td style="padding:2px;text-align: right; vertical-align: top; font-size: {!FontSize};  border-top: 0px #c0c0c0 solid; border-left: 0px #808080 solid; border-right: 1px #808080 solid; border-bottom: 1px #c0c0c0 solid; " >
					<apex:outputText value="${0,number,#0.00}" >
						<apex:param value="{!XXX.totalLow}" />
					</apex:outputText> 						
				</td>
				<td style="padding:2px;text-align: right; vertical-align: top; font-size: {!FontSize}; border-top: 0px #c0c0c0 solid; border-left: 0px #808080 solid; border-right: 1px #808080 solid; border-bottom: 1px #c0c0c0 solid; ">
					<apex:outputText value="${0,number,#0.00}" >
						<apex:param value="{!XXX.totalHigh}" />
					</apex:outputText> 						
				</td>
				<td colspan="3" style=" vertical-align: top; {!BorderColors}" colspan="2"></td>
			</tr>
			
			</table>

		</td>
	</tr>
			
		</apex:repeat>
	</table>
	</body>
</html>
</apex:page>