<apex:page controller="PhonebookSelectionController">
<apex:form >


	<br/>
	<b>Select time period for activities on the Phonebook Report:</b>
	<br/><br/>
	<table>
		<tr>
			<td>Start Date:</td>
			<td style="border-left:#CC0000 3px solid;  padding-left:2px;">
				<apex:inputField value="{!minDateOpp.CloseDate}" />
			</td>
		</tr>
		<tr>
			<td>End Date:</td>
			<td style="border-left:#CC0000 3px solid;  padding-left:2px;">
				<apex:inputField value="{!maxDateOpp.CloseDate}" />
			</td>
		</tr>
		<tr>
			<td>Output format:</td>
			<td>
				<apex:selectList size="1" multiselect="false" value="{!FormatStr}">
					<apex:selectOption itemValue="Pdf" itemLabel="Pdf"/>
					<apex:selectOption itemValue="Excel" itemLabel="Excel"/>
				</apex:selectList>
			</td>
		</tr>
	</table> 
	 <br/><br/>
	<apex:commandButton value="Generate Report" action="{!save}" />
</apex:form>
</apex:page>