<apex:page controller="DSE.DS_SetupWizardController" standardStylesheets="true"  sidebar="true" showheader="true" tabStyle="Account" >

<apex:form >
<c:DS_SetupWizardCommon />
<apex:outputpanel id="WizardButtons1" rendered="{!s_showNewPage==false}">
    <c:DS_SetupWizardButtons />
</apex:outputpanel>

<apex:pageBlock id="Topper"> 
    <apex:pageMessages id="pageMessageBit" /> 
    
    <apex:pageBlockSection columns="1" showHeader="true"  title="Data Sources" collapsible="true" rendered="{!s_showNewPage==false}">
 
    <apex:commandButton value="{!$Label.dse__DS_Wizard_NewDataSource}"   rerender="WizardButtons1,WizardButtons2" >
     <apex:actionSupport event="onclick" action="{!s_NewDoc}" />  
    </apex:commandButton> 
    
    <!-- ************************************************************************************************** -->
    <!-- ************************************** Data Table ************************************************ -->
    <!-- ************************************************************************************************** -->

    <apex:pageBlockTable value="{!ds_c_DataSources}" var="datasource" id="IntTable"  rendered="{!s_showNewPage==false}" rows="{!Int_PageLength}" first="{!Int_PagePosn}" >
        <apex:column width="200" > 
            <apex:facet name="header">Name</apex:facet> 
            <apex:commandlink value="{!$Label.dse__DS_Wizard_Edit}" action="{!s_EditThisDoc}">
                <apex:param name="IDParam" value="{!datasource.id}" /> 
            </apex:commandlink> 
            &nbsp;|&nbsp;
            <apex:commandlink value="{!$Label.dse__DS_Wizard_Delete}" action="{!s_DeleteThisDoc}"  >
                <apex:param name="IDParam" value="{!datasource.id}" />
            </apex:commandlink> &nbsp;&nbsp;
            <apex:OutputText value="{!datasource.Name}" /> 
        </apex:column>
    
        <apex:column width="100" >  
            <apex:facet name="header">Object Class</apex:facet>
            <apex:OutputText value="{!datasource.DSE__DS_Object_Class__c}"/>
        </apex:column>
        <apex:column width="100" >  
            <apex:facet name="header">Dynamic Priority</apex:facet>
            <apex:OutputText value="{!datasource.DSE__DS_Dynamic_Priority__c}"/>
        </apex:column>      
        <apex:column width="200" >  
            <apex:facet name="header">Description</apex:facet>
            <apex:OutputText value="{!datasource.DSE__DS_Description__c}"/>
        </apex:column>
    </apex:pageBlockTable> 
    
    <apex:outputpanel id="IntTableButtons">
        <apex:commandlink value="<<" action="{!s_NextPrevScroll}" rendered="{!Int_PageCount>=Int_PageLength}" rerender="IntTable,IntTableButtons">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Start" />
        </apex:commandlink>&nbsp;&nbsp;&nbsp;
        <apex:commandlink value="Previous" action="{!s_NextPrevScroll}" rerender="IntTable,IntTableButtons" rendered="{!ShowPrevTable1}">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Backwards" /> 
        </apex:commandlink>&nbsp;&nbsp;
        <apex:commandlink value="Next" action="{!s_NextPrevScroll}" rerender="IntTable,IntTableButtons" rendered="{!ShowNextTable1}">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Forwards" />
        </apex:commandlink>
         &nbsp;&nbsp;&nbsp;<apex:commandlink value=">>" action="{!s_NextPrevScroll}" rendered="{!Int_PageCount>=Int_PageLength}" rerender="IntTable,IntTableButtons" >
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="End" />
        </apex:commandlink>
    </apex:outputpanel>

 
    <!-- ************************************************************************************************** -->     
    
    </apex:pageBlockSection> 
    <div style="text-align: center;">
        <apex:commandButton value="{!$Label.dse__DS_Wizard_SaveDataSource}" action="{!s_SaveThisDoc}" rendered="{!s_showNewPage}" rerender="Topper"/>
        <apex:commandButton value="{!$Label.dse__DS_Wizard_Cancel}" action="{!s_CancelSave}"   rendered="{!s_showNewPage}" immediate="true" />  
        <apex:commandButton value="{!if(ShowAdvanced==false,$Label.dse__DS_Wizard_Advanced1,$Label.dse__DS_Wizard_Advanced2)}"  rendered="{!s_showNewPage}" immediate="true" rerender="Topper">
            <apex:param name="ShowAdvanced" value="{!if(ShowAdvanced,false,true)}" assignTo="{!ShowAdvanced}"/>
        </apex:commandButton>  
    </div>
    
    <apex:pageBlockSection columns="2" showHeader="true"  title="Information" collapsible="true" rendered="{!s_showNewPage}">
        <apex:inputField onChange="s_MakeDirty()" value="{!s_300_NewSource.DS_Source.Name   }"/>
        <apex:inputField onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Description__c}"/>
        <apex:inputField onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Object_Class__c}"/>
    </apex:pageBlockSection>
    
    <apex:pageBlockSection columns="2" showHeader="true"  title="Company Information" collapsible="true" rendered="{!s_showNewPage}">
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Name__c.label}"  onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Name}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Website__c.label}"  onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Website}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>        
    </apex:pageBlockSection>
    
    <apex:pageBlockSection columns="2" showHeader="true"  title="Contact Information" collapsible="true" rendered="{!s_showNewPage}">
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_First_Name__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_First_Name}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Last_Name__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Last_Name}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Birthdate__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Birthdate}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Email__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Email}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Phone__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Phone}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Mobile_Phone__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Priority_Mobile_Phone  }"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
    </apex:pageBlockSection>
        
    <apex:pageBlockSection columns="2" showHeader="true"  title="Address Information" collapsible="true" rendered="{!s_showNewPage}">
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Billing_Street__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Billing_Street}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Billing_City__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Billing_City}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Billing_Postal_Code__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Billing_Postal_Code}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Billing_State__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Billing_State}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Country_ISO_Code__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Country_ISO_Code}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>     
    </apex:pageBlockSection>
    
    <apex:pageBlockSection columns="2" showHeader="true"  title="Custom Information" collapsible="true" rendered="{!s_showNewPage}">
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_1__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_1}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_2__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_2}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_3__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_3}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_4__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_4}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_5__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_5}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_6__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_6}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_7__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_7}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_8__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_8}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_9__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_9}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Custom_Field_10__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Weighting_Custom_Field_10}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
    </apex:pageBlockSection>
    
    <apex:pageBlockSection columns="2" showHeader="true"  title="Settings" collapsible="true" rendered="{!s_showNewPage && ShowAdvanced}">
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Enable_for_Search__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Enable_for_Search__c}"/>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Check_for_Duplicates_on_Create__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Check_for_Duplicates_on_Create__c}"/>        
        <apex:selectList size="1" label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Update_Priority_Domain_Address__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Update_Priority_Domain_Address}"><apex:selectOptions value="{!NullScore}"/></apex:selectList>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Dynamic_Priority__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Dynamic_Priority__c}"/>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_Parent_ID__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Priority_Parent_ID__c}"/>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Default_User_ID__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Default_User_ID__c}"/> 
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_External_ID__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Priority_External_ID__c}"/>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Priority_External_Parent_ID__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Priority_External_Parent_ID__c}"/>
        <apex:inputField label="{!$ObjectType.DSE__DS_Sources__c.fields.DSE__DS_Weighting_Ultimate_Parent__c.label}" onChange="s_MakeDirty()" value="{!s_300_Newsource.DS_Source.DSE__DS_Weighting_Ultimate_Parent__c}"/>          
    </apex:pageBlockSection>
    
    <div style="text-align: center;">
        <apex:commandButton value="{!$Label.dse__DS_Wizard_SaveDataSource}" action="{!s_SaveThisDoc}" rendered="{!s_showNewPage}" rerender="Topper"/>  
        <apex:commandButton value="{!$Label.dse__DS_Wizard_Cancel}" action="{!s_CancelSave}" rendered="{!s_showNewPage}" />  
        <apex:commandButton value="{!if(ShowAdvanced==false,$Label.dse__DS_Wizard_Advanced1,$Label.dse__DS_Wizard_Advanced2)}"  rendered="{!s_showNewPage}" immediate="true" rerender="Topper">
            <apex:param name="ShowAdvanced" value="{!if(ShowAdvanced,false,true)}" assignTo="{!ShowAdvanced}"/>
        </apex:commandButton>  
    </div>
    
    <br/>
 
</apex:pageBlock>  


<apex:outputpanel id="WizardButtons2" rendered="{!s_showNewPage==false}">
    <c:DS_SetupWizardButtons />
</apex:outputpanel>

</apex:form>
</apex:page>