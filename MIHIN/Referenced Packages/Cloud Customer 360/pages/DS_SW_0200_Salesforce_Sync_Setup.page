<apex:page controller="DSE.DS_SetupWizardController" standardStylesheets="true"  sidebar="true" showheader="true" tabStyle="Account" >

<apex:form >
<c:DS_SetupWizardCommon />
<apex:outputpanel id="WizardButtons1" rendered="{!s_showNewPage==false}">
    <c:DS_SetupWizardButtons />
</apex:outputpanel>

<apex:pageBlock id="Topper"> 
    <apex:pageMessages id="pageMessageBit" /> 

    <apex:pageBlockSection columns="1" showHeader="true"  title="Salesforce Sync Setup (Field Mapping)" collapsible="true" rendered="{!s_showNewPage==false}">

    <apex:commandButton value="{!$Label.dse__DS_Wizard_NewSynchMapping}" rerender="WizardButtons1,WizardButtons2">
        <apex:actionSupport action="{!s_NewDoc}"  event="onclick" /> 
    </apex:commandButton>   
    
    <!-- ************************************************************************************************** -->
    <!-- ************************************** Data Table ************************************************ -->
    <!-- ************************************************************************************************** --> 
    
 <apex:pageBlockTable value="{!ds_c_syncmap}" var="synchmap"  id="IntTable"  rendered="{!s_showNewPage==false}" rows="{!Int_PageLength}" first="{!Int_PagePosn}" >
        <apex:column width="200" >  
            <apex:facet name="header">Name</apex:facet>
            <apex:commandlink value="{!$Label.dse__DS_Wizard_Edit}" action="{!s_EditThisDoc}">
                <apex:param name="IDParam" value="{!synchmap.id}" /> 
            </apex:commandlink> 
            &nbsp;|&nbsp;
            <apex:commandlink value="{!$Label.dse__DS_Wizard_Delete}" action="{!s_DeleteThisDoc}"  >
                <apex:param name="IDParam" value="{!synchmap.id}" />
            </apex:commandlink> 
             &nbsp;&nbsp;  
             <apex:OutputText value="{!synchmap.Name}" /> 
        </apex:column>

        <apex:column width="100" >  
            <apex:facet name="header">Synchronize</apex:facet>
            <apex:OutputText value="{!synchmap.DSE__Synchronize__c}"/>
        </apex:column>
        <apex:column width="100" >  
            <apex:facet name="header">Keep Data After Object Deletion</apex:facet>
            <apex:OutputText value="{!synchmap.DSE__Keep_Data__c}"/>
        </apex:column>
        <apex:column width="100" >  
            <apex:facet name="header">Quick Cleansing</apex:facet>
            <apex:OutputText value="{!synchmap.DSE__DS_Quick_Cleansing__c}"/>
        </apex:column>
        
    </apex:pageBlockTable>  
    
    
    
    
    
        <apex:outputpanel id="IntTableButtons">
        <apex:commandlink value="<<" action="{!s_NextPrevScroll}" rendered="{!Int_PageCount>=Int_PageLength}" rerender="IntTable,IntTableButtons">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Start" />
        </apex:commandlink>&nbsp;&nbsp;&nbsp;
        <apex:commandlink value="Previous" action="{!s_NextPrevScroll}" rerender="IntTable,IntTableButtons" rendered="{!ShowPrevTable1}">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Backwards" /> 
        </apex:commandlink>&nbsp;&nbsp;
        <apex:commandlink value="Next" action="{!s_NextPrevScroll}" rerender="IntTable,IntTableButtons" rendered="{!ShowNextTable1}">
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="Forwards" />
        </apex:commandlink>
         &nbsp;&nbsp;&nbsp;<apex:commandlink value=">>" action="{!s_NextPrevScroll}" rendered="{!Int_PageCount>=Int_PageLength}" rerender="IntTable,IntTableButtons" >
             <apex:param name="PageToScroll" value="Int" /> 
             <apex:param name="ForwardsBackwards" value="End" />
        </apex:commandlink>
    </apex:outputpanel>

 
    <!-- ************************************************************************************************** -->     
    
    
<!-- ********* Enhanced List Stuff That IS Dead Now
<apex:enhancedList oncomplete="JJ()" type="DS_SF_Synchronization_Settings__c" height="300" rowsPerPage="10"  id="eList" customizable="true"/>

<script>
function JJ()
{
    alert('go')
}
</script>
*********   -->

    </apex:pageBlockSection>    
    
    <!-- ****************************** NEW PAGE ******************* -->
    <div style="text-align: center;">
        <apex:commandButton value="{!$Label.dse__DS_Wizard_SaveSynchMapping}" action="{!s_SaveThisDoc}" rendered="{!s_showNewPage && s_200_NewSynchMap.Name <>''}" rerender="Topper"/>
        <apex:commandButton value="{!$Label.dse__DS_Wizard_Cancel}" action="{!s_CancelSave}"   rendered="{!s_showNewPage}" />  
    </div>
    <apex:pageBlockSection columns="1" showHeader="true"  title="New Synch Mapping" collapsible="true" rendered="{!s_showNewPage}" id="TopField">
        
        <apex:selectList multiselect="false" value="{!s_200_NewSynchMap.Name}"  size="1" rendered="{!s_200_NewSynchMap.Name =''}"> 
                     <apex:actionSupport action="{!s_200_SynchObjectChange}" event="onchange" rerender="objectFields,TopField,Topper"/> 
                     <apex:selectOptions value="{!ObjectList}"></apex:selectOptions>
         </apex:selectList> 
                    
        <apex:OutputText value="{!s_200_NewSynchMap.Name    }" rendered="{!s_200_NewSynchMap.Name <>''}"/>
    </apex:pageBlockSection>

    </apex:pageblock>
    
    <apex:pageBlock id="objectFields"> 
    
    <apex:pageBlockSection columns="2" showHeader="true" rendered="{!s_200_NewSynchMap.Name <>''}" collapsible="true" Title="Contact Information">
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__First_Name__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Last_Name__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Email__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Phone__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Mobile_Phone__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Birthdate__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>     
    </apex:pageBlockSection>
        
    <apex:pageBlockSection columns="2" showHeader="true" rendered="{!s_200_NewSynchMap.Name <>''}" collapsible="true" Title="Company Information">
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Company__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:outputtext value=""/>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Domain__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Website__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>   
    </apex:pageBlockSection>
        
    <apex:pageBlockSection columns="2" showHeader="true" rendered="{!s_200_NewSynchMap.Name <>''}" collapsible="true" Title="Address Information">  
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Billing_Street__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>        
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Billing_City__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>      
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Billing_State__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Billing_Postal_Code__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Billing_Country__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>       
    </apex:pageBlockSection> 
    
    <apex:pageBlockSection columns="2" showHeader="true" rendered="{!s_200_NewSynchMap.Name <>''}" collapsible="true" Title="Custom Information">
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_1__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_2__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_3__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_4__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_5__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_6__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_7__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_8__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_9__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Custom_Field_10__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
    </apex:pageBlockSection>
 
    <apex:pageBlockSection columns="2" showHeader="true" rendered="{!s_200_NewSynchMap.Name <>''}" collapsible="true" Title="Other Information">    
        <apex:inputField onChange="s_MakeDirty()" value="{!s_200_NewSynchMap.Keep_Data__c   }"/> <!--  checkbox -->
        <apex:inputField onChange="s_MakeDirty()" value="{!s_200_NewSynchMap.Synchronize__c }"/> <!--  checkbox -->
        <apex:inputField onChange="s_MakeDirty()" value="{!s_200_NewSynchMap.DS_Quick_Cleansing__c  }"/> <!--  checkbox -->
        <apex:outputtext value=""/>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Parent_ID__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
        <apex:selectList onChange="s_MakeDirty()" multiselect="false" value="{!s_200_NewSynchMap.DSE__Record_Type_ID__c}"  size="1" ><apex:selectOptions value="{!FieldList}"></apex:selectOptions></apex:selectList>
    </apex:pageBlockSection> 
     
        
    <div style="text-align: center;">
        <apex:commandButton value="{!$Label.dse__DS_Wizard_SaveSynchMapping}" action="{!s_SaveThisDoc}" rendered="{!s_showNewPage && s_200_NewSynchMap.Name <>''}" rerender="Topper"/>
        <apex:commandButton value="{!$Label.dse__DS_Wizard_Cancel}" action="{!s_CancelSave}"   rendered="{!s_showNewPage}" /> 
    </div>
    <!-- *********************************************************** -->
        
</apex:pageBlock>
 
<apex:outputpanel id="WizardButtons2" rendered="{!s_showNewPage==false}">
    <c:DS_SetupWizardButtons />
</apex:outputpanel>

