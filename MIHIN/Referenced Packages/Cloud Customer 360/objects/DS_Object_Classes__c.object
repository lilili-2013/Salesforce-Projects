<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <customSettingsVisibility>Public</customSettingsVisibility>
    <description>Cloud MDM: Lists all Objects Classes with their Object Families, e.g. Object Class &quot;Contact&quot; with Object Family &quot;Company&quot;</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>DS_Detect_Duplicates_from_all_Sources__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>When checked, Cloud MDM will detect all potential duplicate records within this Object Class, regardless of the Source of the records. When unchecked, Cloud MDM will only detect duplicate records from the same Source.</description>
        <externalId>false</externalId>
        <inlineHelpText>When checked, Cloud MDM will detect all potential duplicate records within this Object Class, regardless of the Source of the records. When unchecked, Cloud MDM will only detect duplicate records from the same Source.</inlineHelpText>
        <label>Detect Duplicates from all Sources</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>DS_Object_Family__c</fullName>
        <deprecated>false</deprecated>
        <description>The Object Family for this Object Class, e.g. Object Family &quot;Company&quot; for Object Class &quot;Contact&quot;</description>
        <externalId>false</externalId>
        <inlineHelpText>The Object Family for this Object Class, e.g. Object Family &quot;Company&quot; for Object Class &quot;Contact&quot;</inlineHelpText>
        <label>Object Family</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Object Classes</label>
</CustomObject>
