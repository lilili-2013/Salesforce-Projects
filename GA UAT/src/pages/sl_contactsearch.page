    $(function() {
        $('#table').bootstrapTable({
            pagination: true,
            onClickRow: function(r, e) {
                if ((typeof sforce != 'undefined') && (sforce != null)) {
                    sforce.one.navigateToSObject(r.Id, "DETAIL");
                } else {
                    window.location.href = '/' + r.Id;
                }

            }
