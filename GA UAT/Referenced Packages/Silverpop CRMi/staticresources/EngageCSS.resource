}

.personalizationPm ul li a{
	padding: 2px;
	text-decoration:none;
}

.countdown{
	border: none; 
	background-color:#F3F3EC;font-family:'Arial','Helvetica',sans-serif; 
	width:28px;
}

.loadingText{

	font-size: 16pt;
	font-weight:bold;
	margin-left: 350px;
}
