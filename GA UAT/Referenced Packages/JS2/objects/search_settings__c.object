        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>button_custom1_active__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Main controller to Show (checked) or Hide (unchecked) the Custom Action button within the Search Results.  The button will not display if the Custom1 URL is empty even if this button is checked.</description>
        <externalId>false</externalId>
        <inlineHelpText>Controls the Show (checked) / Hide (unchecked) action of the Custom1 Button within the Search Results.  However, if Custom1 URL is empty the button will not appear even if this is checked.</inlineHelpText>
        <label>Button Custom1 Active</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>button_custom1_label__c</fullName>
        <deprecated>false</deprecated>
        <description>Value entered here will be the name of the button within the Search Results.  The name should represent the type of action the button will perform.</description>
        <externalId>false</externalId>
