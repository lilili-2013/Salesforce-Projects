<apex:page standardController="User" sidebar="false" showHeader="false" extensions="ts2.s_ContactFilterController" id="usrPg" cache="false" expires="600">
<script>
function closeWin()
{
parent.window.opener = self;
parent.window.close();
}

function showMessage(message){ alert(message); }
</script>
<apex:form id="topFm">
    <apex:pageBlock title="" id="contactFilterPb">    
      <apex:pageBlockButtons >
		<table>
			<tr>
				<td style="padding-right:20px; width:230px;">
					<apex:commandButton value="Save Field Selection" action="{!saveActionRefresh}" />
                </td>
			</tr>		
		</table>
      </apex:pageBlockButtons>   
    <apex:pageBlockTable id="contactFilterPbTbl" value="{!FieldList}" var="var" cellpadding="6" width="100%" border="0" columnClasses="classCol1" styleClass="resultListTbl">
        <apex:column >
            <apex:facet name="header">&nbsp; </apex:facet>
            <apex:inputCheckbox value="{!var.IsSelected}" /> 
        </apex:column>
        <apex:column >
            <apex:facet name="header">Contact Field &nbsp; &nbsp;</apex:facet>
            <apex:outputText value="{!var.ListItem.label}" />
        </apex:column>
    </apex:pageBlockTable>
</apex:pageBlock>
</apex:form>
</apex:page>